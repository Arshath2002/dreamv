[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "height",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "内边距",
        "showType": "dv_margins",
        "bindControlAttr": "padding",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "组件模式",
        "showType": "dv_radio_group",
        "bindControlAttr": "carouselModel",
        "dictionary": [
          {
            "text": "图片",
            "value": "picture"
          },
          {
            "text": "插槽",
            "value": "slot"
          },
          {
            "text": "组件",
            "value": "component"
          }
        ],
        "desc": "图片模式-静态图片；插槽模式-根据插槽显示不同的组件；组件模式-轮播大屏组件",
        "inCommonUse": true
      },
      {
        "text": "图片模式",
        "bindControlAttr": "backgroundConfigs",
        "showType": "dv_more_img",
        "showExpress": "@[carouselModel=='picture']",
        "inCommonUse": true
      },
      {
        "text": "数据设置",
        "bindControlAttr": "",
        "showExpress": "@[carouselModel=='component']",
        "showType": "dv_dialog",
        "inCommonUse": true,
        "config": {
          "class": "des-dv-data-setting-dialog",
          "content": "DesDVDataSetting",
          "width": "500px",
          "height": "60vh",
          "showBtnType": "common"
        },
        "textCss": {
          "display": "none"
        }
      },
      {
        "text": "插槽设置",
        "bindControlAttr": "slots",
        "showType": "dv_dialog",
        "showExpress": "@[carouselModel!='picture']",
        "inCommonUse": true,
        "config": {
          "content": "DesDVCarouselEdit",
          "width": "1000px",
          "height": "60vh",
          "showBtnType": "common"
        },
        "textCss": {
          "display": "none"
        }
      }
    ]
  },
  {
    "name": "配置设置",
    "attributes": [
      {
        "text": "项目宽",
        "bindControlAttr": "itemWidth",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "itemHeight",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "中心坐标",
        "bindControlAttr": "carouselOptions.xOrigin",
        "showType": "dv_number",
        "itemClass": "float-one-row",
        "desc": "椭圆 x 轴 中心点位置比例 2 代表 1/2 宽度的2分之一",
        "config": {
          "min": 2
        },
        "inCommonUse": true
      },
      {
        "text": " ",
        "hiddenText": true,
        "bindControlAttr": "carouselOptions.yOrigin",
        "showType": "dv_number",
        "itemClass": "float-one-row",
        "desc": "椭圆  y 轴 中心点位置比例 10 代表 1/10 高度的10分之一",
        "config": {
          "min": 2
        },
        "inCommonUse": true
      },
      {
        "text": "长轴",
        "bindControlAttr": "carouselOptions.xRadius",
        "showType": "dv_number",
        "itemClass": "float-one-row",
        "config": {
          "min": 2
        },
        "inCommonUse": true
      },
      {
        "text": "短轴",
        "bindControlAttr": "carouselOptions.yRadius",
        "showType": "dv_number",
        "itemClass": "float-one-row",
        "desc": "椭圆 长半轴 中心点位置比例 2 代表 1/2 宽度的2分之一 ;短半轴 中心点位置比例 6 代表 1/2 宽度的6分之一",
        "config": {
          "min": 2
        },
        "inCommonUse": true
      },
      {
        "text": "速度系数",
        "bindControlAttr": "carouselOptions.speed",
        "showType": "dv_number",
        "itemClass": "float-one-row",
        "desc": "旋转木马的相对速度系数，取值范围：任何正数（即1~），数值越大速度越快，如1表示慢，4表示中等，10表示快",
        "config": {
          "min": 1
        },
        "inCommonUse": true
      },
      {
        "text": "缩放系数",
        "bindControlAttr": "carouselOptions.farScale",
        "showType": "dv_number",
        "itemClass": "float-one-row",
        "desc": "最远缩放比 max=1 min=0.1",
        "config": {
          "min": 0.1,
          "max": 1,
          "step": 0.1
        },
        "inCommonUse": true
      },
      {
        "text": "自动播放",
        "bindControlAttr": "carouselOptions.autoPlay",
        "showType": "dv_radio_group",
        "dictionary": [
          {
            "text": "不自动",
            "value": 0
          },
          {
            "text": "顺时针",
            "value": 1
          },
          {
            "text": "逆时针",
            "value": -1
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "激活类名",
        "bindControlAttr": "carouselOptions.frontItemClass",
        "showType": "dv_text",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]||@[change.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      },
      {
        "text": "懒加载",
        "showType": "dv_switch",
        "bindControlAttr": "isHandlerRefresh"
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]
