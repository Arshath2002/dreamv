[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "showType": "dv_pixel",
        "bindControlAttr": "width",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "showType": "dv_pixel",
        "bindControlAttr": "height",
        "itemClass": "float-one-row",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "边框风格",
        "bindControlAttr": "comType",
        "inCommonUse": true,
        "showType": "dv_dialog",
        "config": {
          "width": "1600px",
          "height": "650px",
          "content": "DesDVBorderStyleSetting",
          "title": "边框风格设置",
          "isClear": "重置",
          "params": {
            "comType": "dsf.dataviewborder1"
          }
        }
      },
      {
        "text": "主色系",
        "bindControlAttr": "mainColor",
        "showType": "dv_color",
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "副色系",
        "bindControlAttr": "viceColor",
        "showType": "dv_color",
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "闪烁",
        "bindControlAttr": "blink",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "动画时长",
        "bindControlAttr": "dur",
        "showExpress": "@[blink]",
        "showType": "dv_number",
        "spacingNumber": 1,
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]