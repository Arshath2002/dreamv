[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "height",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "标题",
        "showType": "dv_switch",
        "bindControlAttr": "isHasHeader",
        "inCommonUse": true
      },
      {
        "text": "名称",
        "showExpress": "@[isHasHeader]",
        "bindControlAttr": "title",
        "spacingNumber": 1,
        "showType": "dv_text",
        "noCopy": true,
        "inCommonUse": true,
        "desc": "<div>支持：路径参数、全局参数、page、弹窗参数、cookie、sessionStorage、localStorage</div><div>示例：@[路径参数.name]</div>"
      },
      {
        "text": "样式",
        "showExpress": "@[isHasHeader]",
        "showType": "dv_text_style",
        "bindControlAttr": "titleTextStyle",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "对齐",
        "showExpress": "@[isHasHeader]",
        "hiddenText": true,
        "showType": "dv_text_align",
        "bindControlAttr": "titleTextAlign",
        "config": {
          "iconType": "textContent"
        },
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "高度",
        "showExpress": "@[isHasHeader]",
        "bindControlAttr": "titleHeight",
        "spacingNumber": 1,
        "showType": "dv_pixel",
        "inCommonUse": true
      },
      {
        "showType": "dv_line",
        "showExpress": "@[isHasHeader]",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "副标题",
        "isFirstText": true,
        "showExpress": "@[isHasHeader]",
        "bindControlAttr": "showSubTitle",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "内容",
        "bindControlAttr": "subTitle",
        "showExpress": "@[isHasHeader && showSubTitle]",
        "spacingNumber": 1,
        "showType": "dv_text",
        "noCopy": true,
        "inCommonUse": true,
        "desc": "<div>支持：路径参数、全局参数、page、弹窗参数、cookie、sessionStorage、localStorage</div><div>示例：@[路径参数.name]</div>"
      },
      {
        "text": "样式",
        "showType": "dv_text_style",
        "bindControlAttr": "subTitleTextStyle",
        "spacingNumber": 1,
        "showExpress": "@[isHasHeader && showSubTitle]",
        "inCommonUse": true
      },
      {
        "showType": "dv_line",
        "spacingNumber": 1,
        "showExpress": "@[isHasHeader && showSubTitle]",
        "inCommonUse": true
      },
      {
        "text": "更多按钮",
        "isFirstText": true,
        "bindControlAttr": "moreShow",
        "showExpress": "@[isHasHeader]",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "图标选择",
        "bindControlAttr": "moreIcon",
        "showExpress": "@[isHasHeader && moreShow]",
        "showType": "dv_more_icon",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "图标颜色",
        "bindControlAttr": "iconColor",
        "showExpress": "@[isHasHeader && moreShow]",
        "showType": "dv_color",
        "spacingNumber": 1,
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "图标大小",
        "bindControlAttr": "iconSize",
        "showExpress": "@[isHasHeader && moreShow]",
        "showType": "dv_number",
        "config": {
          "unit": "px"
        },
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "标题文字",
              "showExpress": "@[isHasHeader]",
              "bindControlAttr": "titlePadding"
            },
            {
              "text": "副标题",
              "showExpress": "@[isHasHeader && showSubTitle]",
              "bindControlAttr": "subTitlePadding"
            },
            {
              "text": "内容内边距",
              "bindControlAttr": "contentPadding"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "整体背景",
        "inCommonUse": true
      },
      {
        "text": "颜色",
        "bindControlAttr": "totalBackgroundConfig",
        "spacingNumber": 1,
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "totalBackgroundConfig",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "spacingNumber": 1,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "page"
          ]
        }
      },
      {
        "text": "外阴影",
        "bindControlAttr": "boxOutsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset"
        },
        "inCommonUse": true
      },
      {
        "text": "内阴影",
        "bindControlAttr": "boxInsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "inset"
        },
        "inCommonUse": true
      },
      {
        "text": "标题背景",
        "inCommonUse": true
      },
      {
        "text": "颜色",
        "bindControlAttr": "titleBackgroundConfig",
        "spacingNumber": 1,
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "titleBackgroundConfig",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "spacingNumber": 1,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "model",
            "title"
          ]
        }
      },
      {
        "text": "内容背景",
        "inCommonUse": true
      },
      {
        "text": "颜色",
        "bindControlAttr": "contentBackground",
        "spacingNumber": 1,
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "contentBackground",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "spacingNumber": 1,
        "config": {
          "limitImg": 1
        }
      }
    ]
  },
  {
    "name": "边框设置",
    "attributes": [
      {
        "text": "",
        "bindControlAttr": "borderConfig",
        "showType": "dv_border",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      }
    ]
  },
  {
    "name": "插槽",
    "attributes": [
      {
        "text": "左上插槽",
        "isFirstText": true,
        "bindControlAttr": "isSlotTopLeft",
        "showType": "dv_switch"
      },
      {
        "text": "宽",
        "bindControlAttr": "slotTopLeftWidth",
        "showExpress": "@[isSlotTopLeft == true]",
        "itemClass": "float-one-row",
        "showType": "dv_pixel",
        "spacingNumber": 1
      },
      {
        "text": "高",
        "bindControlAttr": "slotTopLeftHeight",
        "showExpress": "@[isSlotTopLeft == true]",
        "itemClass": "float-one-row ml20",
        "showType": "dv_pixel"
      },
      {
        "text": "右上插槽",
        "isFirstText": true,
        "bindControlAttr": "isSlotTopRight",
        "showType": "dv_switch"
      },
      {
        "text": "宽",
        "bindControlAttr": "slotTopRightWidth",
        "showExpress": "@[isSlotTopRight == true]",
        "itemClass": "float-one-row",
        "showType": "dv_pixel",
        "spacingNumber": 1
      },
      {
        "text": "高",
        "bindControlAttr": "slotTopRightHeight",
        "showExpress": "@[isSlotTopRight == true]",
        "itemClass": "float-one-row ml20",
        "showType": "dv_pixel"
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]