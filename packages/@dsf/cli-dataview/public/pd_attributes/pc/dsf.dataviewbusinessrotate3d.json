[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "高度",
        "bindControlAttr": "height",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "内边距",
        "showType": "dv_margins",
        "inCommonUse": true,
        "bindControlAttr": "padding"
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "映射字段",
        "showType": "dv_dialog",
        "bindControlAttr": "mapRelations",
        "dialogType": "relation",
        "inCommonUse": true,
        "config": {
          "title": "设置",
          "content": "DesDVMapRelationSetting",
          "width": "1100px",
          "height": "60vh",
          "buttonText": "设置"
        }
      }
    ]
  },
  {
    "name": "样式设置",
    "attributes": [
      {
        "text": "模块背景",
        "showType": "dv_switch",
        "bindControlAttr": "isCustom",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "scaleGroundConfigs",
        "showType": "dv_more_img",
        "showExpress": "@[isCustom]",
        "hiddenText": true,
        "inCommonUse": true
      },
      {
        "text": "模块大小",
        "bindControlAttr": "size",
        "showType": "dv_number",
        "config": {
          "min":10
        },
        "inCommonUse": true
      },

      {
        "text": "翻转角度",
        "showType": "dv_number",
        "bindControlAttr": "rotateX",
        "inCommonUse": true,
        "config": {
          "min": 0,
          "max": 89,
          "unit": "deg"
        }
      },
      {
        "text": "倾斜角度",
        "showType": "dv_number",
        "bindControlAttr": "rotateZ",
        "inCommonUse": true,
        "config": {
          "max": 89,
          "unit": "deg"
        }
      },
      {
        "text": "模块缩放",
        "showType": "dv_number",
        "bindControlAttr": "farScale",
        "inCommonUse": true,
        "config": {
          "min": 0.5,
          "max": 1,
          "step": 0.1
        }
      },
      {
        "text": "字体样式",
        "showType": "dv_text_style",
        "bindControlAttr": "textStyle",
        "inCommonUse": true
      },
      {
        "text": "自动播放",
        "bindControlAttr": "autoPlay",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "时间间隔",
        "bindControlAttr": "timelineTimeDistance",
        "showExpress": "@[autoPlay]",
        "showType": "dv_number",
        "inCommonUse": true,
        "config": {
          "min": 1
        }
      },
      {
        "text": "运动速度",
        "bindControlAttr": "speed",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "轨道",
        "bindControlAttr": "isTrack",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "轨道宽度",
        "bindControlAttr": "trackWidth",
        "showType": "dv_number",
        "showExpress": "@[isTrack]",
        "inCommonUse": true,
        "config": {
          "min": 1
        }
      },
      {
        "text": "轨道颜色",
        "showType": "dv_color",
        "showExpress": "@[isTrack]",
        "bindControlAttr": "trackColor",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "插槽样式",
    "attributes": [
      {
        "text": "是否开启页面插槽",
        "bindControlAttr": "isSlotFullPanel",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "是否开启环绕效果",
        "bindControlAttr": "isAround",
        "showExpress": "@[isSlotFullPanel == true]",
        "showType": "boolean",
        "inCommonUse": true
      },
      {
        "text": "插槽高度",
        "bindControlAttr": "slotHeight",
        "showExpress": "@[isSlotFullPanel == true]",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "插槽宽度",
        "bindControlAttr": "slotWidth",
        "showExpress": "@[isSlotFullPanel == true]",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "插槽相对于顶部偏移",
        "bindControlAttr": "slotTop",
        "showExpress": "@[isSlotFullPanel == true]",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "插槽相对于左侧偏移",
        "bindControlAttr": "slotLeft",
        "showExpress": "@[isSlotFullPanel == true]",
        "showType": "dv_text",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "颜色",
        "bindControlAttr": "backgroundConfig",
        "spacingNumber": 1,
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "backgroundConfig",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "spacingNumber": 1,
        "config": {
          "limitImg": 1,
          "backgroundTypes": ["page"]
        }
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      },
      {
        "text": "懒加载",
        "showType": "dv_switch",
        "bindControlAttr": "isHandlerRefresh"
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]
