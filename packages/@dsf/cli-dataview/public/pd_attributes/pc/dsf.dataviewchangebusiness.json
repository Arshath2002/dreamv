[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "showType": "dv_pixel",
        "bindControlAttr": "height",
        "itemClass": "float-one-row",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "设置",
