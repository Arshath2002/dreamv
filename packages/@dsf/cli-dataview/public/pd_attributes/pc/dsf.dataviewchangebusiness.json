        "config": {
          "title": "业务组件选择",
          "content": "DesDVBusinessComponents",
          "width": "1000px",
          "multiple": false,
          "height": "60vh",
          "isClear": true,
          "clearClose": false,
          "buttonText": "设置"
        }
      }
    ]
  },
  {
