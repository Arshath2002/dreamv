[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "showType": "dv_pixel",
        "bindControlAttr": "height",
        "itemClass": "float-one-row",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "上传图片",
        "showType": "dv_switch",
        "bindControlAttr": "staticImage",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "backgroundConfigs",
        "showType": "dv_more_img",
        "showExpress": "@[staticImage]",
        "config": {
          "isEffect": false,
          "isFilter": false,
          "hasStyle":false,
          "backgroundTypes": []
        },
        "hiddenText": true,
        "inCommonUse": true
      },
      {
        "text": "映射字段",
        "bindControlAttr": "mapRelations",
        "showExpress": "@[!staticImage]",
        "showType": "dv_dialog",
        "dialogType": "relation",
        "inCommonUse": true,
        "config": {
          "title": "设置",
          "content": "DesDVMapRelationSetting",
          "width": "1100px",
          "height": "60vh",
          "showBtnType": "common"
        },
        "textCss": {
          "display": "none"
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "幻灯片高",
        "showType": "dv_number",
        "bindControlAttr": "imageHeight",
        "inCommonUse": true
      },
      {
        "text": "间隔角度",
        "showType": "dv_number",
        "bindControlAttr": "gap",
        "config": {
          "unit": "deg",
          "min": 0
        },
        "inCommonUse": true
      },
      {
        "text": "显示名称",
        "showType": "dv_switch",
        "bindControlAttr": "showName",
        "inCommonUse": true
      },
      {
        "text": "名称高度",
        "showType": "dv_number",
        "bindControlAttr": "textHeight",
        "showExpress": "@[showName]",
        "inCommonUse": true
      },
      {
        "text": "样式",
        "showType": "dv_text_style",
        "bindControlAttr": "textStyle",
        "showExpress": "@[showName]",
        "config": {
          "isShowFontWeight": false,
          "isShowItalics": false,
          "isShowUnderline": false,
          "isShowFontFamily": false,
          "isShowShadow": false,
          "isMarginTop": false
        },
        "hiddenText": true,
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "旋转设置",
    "attributes": [
      {
        "text": "旋转方向",
        "showType": "dv_radio_group",
        "bindControlAttr": "rotate.autoRotateDirection",
        "dictionary": [
          {
            "text": "顺时针",
            "value": "cw"
          },
          {
            "text": "逆时针",
            "value": "cww"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "旋转速度",
        "showType": "dv_number",
        "bindControlAttr": "rotate.autoRotateSpeed",
        "config": {
          "min": 1
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "跳转",
    "attributes": [
      {
        "text": "点击跳转",
        "showType": "dv_radio_group",
        "bindControlAttr": "openConfig.interactType",
        "showExpress": "@[!staticImage]",
        "dictionary": [
          {
            "text": "打开页面",
            "value": "1"
          },
          {
            "text": "打开外部链接",
            "value": "2"
          },
          {
            "text": "大屏弹框",
            "value": "3"
          },
          {
            "text": "无",
            "value": "-1"
          }
        ],
        "itemClass": "property-item-radio-group_minute‌",
        "inCommonUse": true
      },
      {
        "text": "打开方式",
        "showType": "dv_radio_group",
        "bindControlAttr": "openConfig.isOuter",
        "showExpress": "@[!staticImage && (openConfig.interactType == 1 || openConfig.interactType == 2)]",
        "dictionary": [
          {
            "text": "当前窗口",
            "value": false
          },
          {
            "text": "新窗口",
            "value": true
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "弹窗类型",
        "showType": "dv_select",
        "bindControlAttr": "openConfig.dialogType",
        "showExpress": "@[!staticImage && openConfig.interactType == 3]",
        "loadExceFunction": "addDvDialogIds",
        "dictionary": [],
        "inCommonUse": true
      },
      {
        "text": "弹窗标题",
        "showType": "dv_text",
        "bindControlAttr": "openConfig.dialogTitle",
        "showExpress": "@[!staticImage && openConfig.interactType == 3]",
        "inCommonUse": true
      },
      {
        "text": "弹窗宽",
        "showType": "dv_pixel",
        "bindControlAttr": "openConfig.dialogWidth",
        "showExpress": "@[!staticImage && openConfig.interactType == 3]",
        "inCommonUse": true
      },
      {
        "text": "弹窗高",
        "showType": "dv_pixel",
        "bindControlAttr": "openConfig.dialogHeight",
        "showExpress": "@[!staticImage && openConfig.interactType == 3]",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      },
      {
        "text": "懒加载",
        "showType": "dv_switch",
        "bindControlAttr": "isHandlerRefresh"
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]
