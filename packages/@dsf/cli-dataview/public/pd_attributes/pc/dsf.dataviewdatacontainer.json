[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "名称",
        "bindControlAttr": "text",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "名称颜色",
        "bindControlAttr": "textColor",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "使用场景",
        "showType": "dv_radio_group",
        "bindControlAttr": "scene",
        "dictionary": [
          {
            "text": "gis地图",
            "value": "gis"
          },
          {
            "text": "普通",
            "value": "common"
          },
          {
            "text": "自定义",
            "value": "custom"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "事件名称",
        "showExpress": "@[scene === 'custom']",
        "bindControlAttr": "eventName",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "数据设置",
        "bindControlAttr": "dataSet",
        "showType": "dv_dialog",
        "inCommonUse": true,
        "config": {
          "class": "des-dv-data-setting-dialog",
          "content": "DesDVDataSetting",
          "width": "500px",
          "height": "60vh"
        }
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      },
      {
        "text": "懒加载",
        "showType": "dv_switch",
        "bindControlAttr": "isHandlerRefresh"
      }
    ]
  }
]