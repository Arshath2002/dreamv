[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "宽度",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ",
        "inCommonUse": true
      },
      {
        "text": "高度",
        "bindControlAttr": "height",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "形状",
        "showType": "dv_radio_group",
        "bindControlAttr": "shape",
        "dictionary": [
          {
            "text": "圆",
            "value": "circle"
          },
          {
            "text": "矩形",
            "value": "rect"
          },
          {
            "text": "三角形",
            "value": "triangle"
          },
          {
            "text": "菱形",
            "value": "diamond"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "方向",
        "showType": "dv_radio_group",
        "showExpress": "@[shape === 'triangle']",
        "bindControlAttr": "triangleDirection",
        "dictionary": [
          {
            "text": "上",
            "value": "top"
          },
          {
            "text": "右",
            "value": "right"
          },
          {
            "text": "下",
            "value": "bottom"
          },
          {
            "text": "左",
            "value": "left"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "",
        "hiddenText": true,
        "showType": "dv_radio_group",
        "bindControlAttr": "fakeSelect",
        "dictionary": [
          {
            "text": "外层",
            "value": "outer"
          },
          {
            "text": "内层",
            "value": "inner"
          }
        ],
        "config": {
          "autoFit": true,
          "stroke": true
        },
        "inCommonUse": true
      },
      {
        "text": "大小",
        "bindControlAttr": "itemWidth",
        "showType": "dv_number",
        "showExpress": "@[fakeSelect==='outer' && shape !== 'rect']",
        "config": {
          "unit": "px"
        },
        "inCommonUse": true
      },
      {
        "text": "宽度",
        "bindControlAttr": "itemWidth",
        "showType": "dv_number",
        "itemClass": "float-one-row",
        "showExpress": "@[fakeSelect==='outer' && shape === 'rect']",
        "config": {
          "unit": "px"
        },
        "inCommonUse": true
      },
      {
        "text": "高度",
        "bindControlAttr": "itemHeight",
        "showType": "dv_number",
        "itemClass": "float-one-row ml20",
        "showExpress": "@[fakeSelect==='outer' && shape === 'rect']",
        "config": {
          "unit": "px"
        },
        "inCommonUse": true
      },
      {
        "text": "边框样式",
        "bindControlAttr": "borderConfig",
        "showExpress": "@[fakeSelect==='outer']",
        "showType": "dv_border",
        "inCommonUse": true,
        "config": {
          "isShowTop": false,
          "isShowRight": false,
          "isShowBottom": false,
          "isShowLeft": false,
          "isDisabledAll": true
        }
      },
      {
        "text": "阴影",
        "bindControlAttr": "outerShadowConfig",
        "showExpress": "@[fakeSelect==='outer']",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset",
          "desc": ""
        },
        "inCommonUse": true
      },
      {
        "text": "大小",
        "bindControlAttr": "itemInnerWidth",
        "showType": "dv_number",
        "showExpress": "@[fakeSelect==='inner' && shape !== 'rect']",
        "config": {
          "unit": "px"
        },
        "inCommonUse": true
      },
      {
        "text": "宽度",
        "bindControlAttr": "itemInnerWidth",
        "showType": "dv_number",
        "itemClass": "float-one-row",
        "showExpress": "@[fakeSelect==='inner' && shape === 'rect']",
        "config": {
          "unit": "px"
        },
        "inCommonUse": true
      },
      {
        "text": "高度",
        "bindControlAttr": "itemInnerHeight",
        "showType": "dv_number",
        "itemClass": "float-one-row ml20",
        "showExpress": "@[fakeSelect==='inner' && shape === 'rect']",
        "config": {
          "unit": "px"
        },
        "inCommonUse": true
      },
      {
        "text": "背景色",
        "bindControlAttr": "itemInnerColor",
        "showType": "dv_color",
        "showExpress": "@[fakeSelect==='inner']",
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "阴影",
        "bindControlAttr": "innerShadowConfig",
        "showExpress": "@[fakeSelect==='inner']",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset",
          "desc": ""
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "动画",
    "attributes": [
      {
        "text": "动画",
        "bindControlAttr": "animationConfig.flag",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "持续时间",
        "showType": "dv_number",
        "bindControlAttr": "animationConfig.duration",
        "showExpress": "@[animationConfig.flag]",
        "spacingNumber": 1,
        "config": {
          "min": 1,
          "unit": "s"
        },
        "inCommonUse": true
      },
      {
        "text": "循环",
        "bindControlAttr": "animationConfig.isCycle",
        "showExpress": "@[animationConfig.flag]",
        "showType": "dv_switch",
        "spacingNumber": 1,
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "颜色",
        "bindControlAttr": "backgroundConfig",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "backgroundConfig",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1
        }
      },
      {
        "text": "外阴影",
        "bindControlAttr": "boxOutsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset"
        },
        "inCommonUse": true
      },
      {
        "text": "内阴影",
        "bindControlAttr": "boxInsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "inset"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]