[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "高度",
        "bindControlAttr": "height",
        "showType": "dv_pixel",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "组件设置",
    "attributes": [
      {
        "text": "全屏样式",
        "showType": "dv_switch",
        "bindControlAttr": "isDesignFull",
        "inCommonUse": true
      },
      {
        "text": "展示背景",
        "showType": "dv_switch",
        "bindControlAttr": "isShowBg",
        "inCommonUse": true
      },
      {
        "text": "背景",
        "bindControlAttr": "background",
        "showExpress": "@[isShowBg === true]",
        "inCommonUse": true,
        "showType": "dv_dialog",
        "config": {
          "width": "950px",
          "height": "600px",
          "content": "DesDVBgSettingExtend",
          "title": "背景设置",
          "isClear": "重置",
          "params": {
            "onlyImage": true,
            "backgroundTypes": []
          }
        }
      },
      {
        "text": "背景宽",
        "itemClass": "float-one-row",
        "showType": "dv_pixel",
        "showExpress": "@[isShowBg === true]",
        "bindControlAttr": "bgWidth",
        "inCommonUse": true
      },
      {
        "text": "背景高",
        "itemClass": "float-one-row ml20",
        "showType": "dv_pixel",
        "showExpress": "@[isShowBg === true]",
        "bindControlAttr": "bgHeight",
        "inCommonUse": true
      },
      {
        "text": "全屏图标",
        "showType": "icon",
        "bindControlAttr": "fullIcon",
        "inCommonUse": true
      },
      {
        "text": "退出图标",
        "showType": "icon",
        "bindControlAttr": "noFullIcon",
        "inCommonUse": true
      },
      {
        "text": "图标大小",
        "showType": "dv_text",
        "bindControlAttr": "iconFontSize",
        "inCommonUse": true
      },
      {
        "text": "图标颜色",
        "showType": "dv_color",
        "bindControlAttr": "iconColor",
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "图标位置",
        "bindControlAttr": "iconPosition",
        "showType": "dv_margins",
        "inCommonUse": true
      },
      {
        "text": "显示文字",
        "showType": "dv_switch",
        "bindControlAttr": "isShowText",
        "inCommonUse": true
      },
      {
        "text": "文字样式",
        "showType": "dv_text_style",
        "showExpress": "@[isShowText === true]",
        "bindControlAttr": "fullTextStyle",
        "inCommonUse": true
      },
      {
        "text": "全屏文字",
        "showType": "dv_text",
        "showExpress": "@[isShowText === true]",
        "bindControlAttr": "fullText",
        "inCommonUse": true
      },
      {
        "text": "退出文字",
        "showType": "dv_text",
        "showExpress": "@[isShowText === true]",
        "bindControlAttr": "noFullText",
        "inCommonUse": true
      },
      {
        "text": "文字位置",
        "bindControlAttr": "fullTextPosition",
        "showExpress": "@[isShowText === true]",
        "showType": "dv_margins",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]