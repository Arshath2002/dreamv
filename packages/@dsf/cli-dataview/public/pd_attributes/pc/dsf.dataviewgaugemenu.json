[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "height",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "菜单配置",
        "hiddenText": true,
        "textCss": {
          "min-width": 0
        },
        "bindControlAttr": "staticData",
        "showType": "dv_dialog",
        "inCommonUse": true,
        "config": {
          "content": "DesDVCascadeMenuEditor",
          "width": "1300px",
          "height": "60vh",
          "showBtnType": "common",
           "menuType":"gauge"
        }
      },
      {
        "text": "默认高亮菜单",
        "bindControlAttr": "defaultHighlight",
        "showType": "dv_text",
        "desc": "菜单标识",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "根菜单数目",
        "bindControlAttr": "rootNumber",
        "showType": "dv_number",
        "inCommonUse": true,
        "config": {
          "min": 3
        }
      },
      {
        "text": "内径偏移量",
        "bindControlAttr": "offsetPadding",
        "showType": "dv_number",
        "inCommonUse": true
      },
      {
        "text": "底部偏移量",
        "bindControlAttr": "btOffset",
        "showType": "dv_number",
        "inCommonUse": true
      },
      {
        "text": "菜单宽度数",
        "bindControlAttr": "widths",
        "showType": "dv_text",
        "desc": "逗号隔开，由内向外，代表1-n级菜单大小",
        "inCommonUse": true
      },
      {
        "text": "弧度分割数",
        "bindControlAttr": "divisionLen",
        "showType": "dv_text",
        "desc": "逗号隔开，由内向外，代表同意弧度中的菜单数",
        "inCommonUse": true
      },
      {
        "text": "",
        "bindControlAttr": "styles",
        "showType": "dv_gauge_menu",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      }
    ]
  },

  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]
