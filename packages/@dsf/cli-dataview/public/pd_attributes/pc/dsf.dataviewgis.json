[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "地图类型",
        "showType": "dv_radio_group",
        "bindControlAttr": "mapType",
        "dictionary": [
          {
            "text": "高德",
            "value": "AMap"
          },

          {
            "text": "百度",
            "value": "BMap"
          },
          {
            "text": "测绘院",
            "value": "MineMap"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "",
        "bindControlAttr": "",
        "showType": "dv_gis_info",
        "inCommonUse": true
      },
      {
        "text": "一张图运行",
        "bindControlAttr": "openGisPic",
        "showType": "dv_switch"
      },
      {
        "text": "一张图运行入口地址",
        "bindControlAttr": "enterPath",
        "showExpress": "@[openGisPic]",
        "showType": "dv_text"
      }
    ]
  },
  {
    "name": "数据设置",
    "attributes": [
      {
        "text": "数据配置",
        "bindControlAttr": "layerStyles",
        "showExpress": "@[designerLoadMap]",
        "showType": "dv_dialog",
        "dialogType": "relation",
        "inCommonUse": true,
        "config": {
          "content": "DesDVGisRelation",
          "width": "1300px",
          "height": "60vh",
          "showBtnType": "common"
        },
        "textCss": {
          "display": "none"
        }
      }
    ]
  },
  {
    "name": "图层样式",
    "attributes": [
      {
        "text": "",
        "bindControlAttr": "",
        "showType": "dv_gis_layer",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "图例设置",
    "attributes": [
      {
        "text": "",
        "bindControlAttr": "legend",
        "showType": "dv_gis_legend",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "测绘院地图配置",
    "attributes": [
      {
        "text": "域名地址",
        "showExpress": "@[mapType ==='MineMap']",
        "bindControlAttr": "MineMapConfig.map_domainUrl",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "矢量数据",
        "showExpress": "@[mapType ==='MineMap']",
        "bindControlAttr": "MineMapConfig.map_dataDomainUrl",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "数据服务",
        "showExpress": "@[mapType ==='MineMap']",
        "bindControlAttr": "MineMapConfig.map_serverDomainUrl",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "底图地址",
        "showExpress": "@[mapType ==='MineMap']",
        "bindControlAttr": "MineMapConfig.map_spriteUrl",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "后台服务",
        "showExpress": "@[mapType ==='MineMap']",
        "bindControlAttr": "MineMapConfig.map_serviceUrl",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "搜索服务",
        "showExpress": "@[mapType ==='MineMap']",
        "bindControlAttr": "MineMapConfig.mineMapSearchUrl",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "key",
        "showExpress": "@[mapType ==='MineMap']",
        "bindControlAttr": "MineMapConfig.key",
        "showType": "dv_text",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高德地图配置",
    "attributes": [
      {
        "text": "安全码",
        "showExpress": "@[mapType ==='AMap']",
        "bindControlAttr": "AMapConfig.securityJsCode",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "key",
        "showExpress": "@[mapType ==='AMap']",
        "bindControlAttr": "AMapConfig.key",
        "showType": "dv_text",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "百度地图配置",
    "attributes": [
      {
        "text": "浏览器端AK",
        "showExpress": "@[mapType ==='BMap']",
        "bindControlAttr": "BMapConfig.bMapWebAK",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "服务器端AK",
        "showExpress": "@[mapType ==='BMap']",
        "bindControlAttr": "BMapConfig.bMapServiceAK",
        "showType": "dv_text",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "插槽样式",
    "attributes": [
      {
        "text": "右上插槽",
        "bindControlAttr": "isSlotTopRight",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "宽度",
        "bindControlAttr": "slotTopRightWidth",
        "showExpress": "@[isSlotTopRight == true]",
        "itemClass": "float-one-row",
        "showType": "dv_pixel",
        "inCommonUse": true
      },
      {
        "text": "高度",
        "bindControlAttr": "slotTopRightHeight",
        "showExpress": "@[isSlotTopRight == true]",
        "itemClass": "float-one-row ml20",
        "showType": "dv_pixel",
        "inCommonUse": true
      },
      {
        "text": "右下插槽",
        "bindControlAttr": "isSlotBottomRight",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "宽度",
        "bindControlAttr": "slotBottomRightWidth",
        "showExpress": "@[isSlotBottomRight == true]",
        "itemClass": "float-one-row",
        "showType": "dv_pixel",
        "inCommonUse": true
      },
      {
        "text": "高度",
        "bindControlAttr": "slotBottomRightHeight",
        "showExpress": "@[isSlotBottomRight == true]",
        "itemClass": "float-one-row ml20",
        "showType": "dv_pixel",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "配置总览",
        "bindControlAttr": "layerStyles",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDVCustomGisEdit",
          "width": "1000px",
          "height": "60vh"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]
