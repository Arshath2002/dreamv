[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "height",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "映射字段",
        "showType": "dv_dialog",
        "hiddenText": true,
        "dialogType":"relation",
        "textCss": {
          "min-width": 0
        },
        "bindControlAttr": "mapRelations",
        "inCommonUse": true,
        "config": {
          "title": "设置",
          "content": "DesDVMapRelationSetting",
          "width": "1100px",
          "height": "60vh",
          "buttonText": "设置",
          "showBtnType": "common"
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "排列",
        "showType": "dv_radio_group",
        "bindControlAttr": "layoutType",
        "dictionary": [
          {
            "text": "一行",
            "value": "hor"
          },
          {
            "text": "两行",
            "value": "vec"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "纵向信息插槽",
        "showType": "dv_text",
        "bindControlAttr": "vecSlotName",
        "showExpress": "@[layoutType === 'vec']"
      },
      {
        "text": "间距",
        "showType": "dv_number",
        "bindControlAttr": "lineMargin",
        "config": {
          "unit": "px"
        },
        "inCommonUse": true
      },
      {
        "text": "启用色值",
        "showType": "dv_switch",
        "bindControlAttr": "colorIsRelative",
        "inCommonUse": true
      },
      {
        "text": "名称",
        "showType": "dv_text",
        "bindControlAttr": "nameFormatter",
        "inCommonUse": true
      },
      {
        "text": "宽",
        "showType": "dv_pixel",
        "bindControlAttr": "nameTextStyle.width",
        "showExpress": "@[layoutType === 'hor']",
        "inCommonUse": true
      },
      {
        "text": "样式",
        "showType": "dv_text_style",
        "bindControlAttr": "nameTextStyle",
        "showExpress": "@[!colorIsRelative]",
        "spacingNumber": 1,
        "config": {
          "picker": {
            "type": "color"
          }
        },
        "inCommonUse": true
      },
      {
        "text": "样式",
        "showType": "dv_text_style",
        "bindControlAttr": "nameTextStyle",
        "spacingNumber": 1,
        "showExpress": "@[colorIsRelative]",
        "config": {
          "isShowColor": false,
          "picker": {
            "type": "color"
          }
        },
        "inCommonUse": true
      },
      {
        "text": "样式",
        "showType": "dv_text_align",
        "hiddenText": true,
        "bindControlAttr": "nameTextStyle.hAlign",
        "showExpress": "@[layoutType === 'hor']",
        "config": {
          "isShowVAlign": false,
          "iconType": "textContent"
        },
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "showType": "dv_line",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "数值",
        "showType": "dv_text",
        "bindControlAttr": "valueFormatter",
        "inCommonUse": true
      },
      {
        "text": "宽",
        "showType": "dv_pixel",
        "showExpress": "@[layoutType === 'hor']",
        "bindControlAttr": "valueTextStyle.width",
        "inCommonUse": true
      },
      {
        "text": "样式",
        "showType": "dv_text_style",
        "bindControlAttr": "valueTextStyle",
        "spacingNumber": 1,
        "showExpress": "@[!colorIsRelative]",
        "config": {
          "picker": {
            "type": "color"
          }
        },
        "inCommonUse": true
      },
      {
        "text": "样式",
        "showType": "dv_text_style",
        "bindControlAttr": "valueTextStyle",
        "spacingNumber": 1,
        "showExpress": "@[colorIsRelative]",
        "config": {
          "isShowColor": false,
          "picker": {
            "type": "color"
          }
        },
        "inCommonUse": true
      },
      {
        "text": "样式",
        "showType": "dv_text_align",
        "hiddenText": true,
        "bindControlAttr": "valueTextStyle.hAlign",
        "showExpress": "@[layoutType === 'hor']",
        "config": {
          "isShowVAlign": false,
          "iconType": "textContent"
        },
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "showType": "dv_line",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "名称外边距",
              "bindControlAttr": "nameTextStyle.margin"
            },
            {
              "text": "值外边距",
              "bindControlAttr": "valueTextStyle.margin"
            }
          ]
        }
      },
      {
        "text": "条形呈现",
        "showType": "dv_radio_group",
        "bindControlAttr": "thingShowType",
        "dictionary": [
          {
            "text": "条线",
            "value": "line"
          },
          {
            "text": "图标",
            "value": "icon"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "图标大小",
        "showType": "dv_number",
        "showExpress": "@[thingShowType==='icon']",
        "bindControlAttr": "iconSize",
        "config": {
          "unit": "px"
        },
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "图标设置",
        "bindControlAttr": "icons",
        "showExpress": "@[thingShowType==='icon']",
        "showType": "dv_more_icon",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "宽",
        "showExpress": "@[thingShowType==='line']",
        "bindControlAttr": "lineTextStyle.width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "高",
        "showExpress": "@[thingShowType==='line']",
        "bindControlAttr": "lineTextStyle.height",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "线条间隙",
        "showType": "dv_pixel",
        "showExpress": "@[thingShowType==='line']",
        "bindControlAttr": "lineTextStyle.gap",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "动效设置",
        "bindControlAttr": "animateConfig.flag",
        "showExpress": "@[thingShowType==='line']",
        "showType": "dv_switch",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "时长",
        "showType": "dv_number",
        "showExpress": "@[thingShowType==='line' && animateConfig.flag]",
        "bindControlAttr": "animateConfig.dur",
        "config": {
          "unit": "ms"
        },
        "spacingNumber": 2,
        "inCommonUse": true
      },
      {
        "showType": "dv_line",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "色值列表",
        "bindControlAttr": "colors",
        "showType": "dv_more_color",
        "config": {
          "type": [
            "color",
            "linear"
          ],
          "typeOptions": [
            {
              "text": "单色",
              "value": "color"
            },
            {
              "text": "线性渐变",
              "value": "linear"
            }
          ]
        },
        "inCommonUse": true
      },
      {
        "text": "底色",
        "showType": "dv_color",
        "bindControlAttr": "thingBaseColor",
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "计算%",
        "bindControlAttr": "openAutoCalcRatio",
        "showType": "dv_radio_group",
        "dictionary": [
          {
            "text": "自动",
            "value": true
          },
          {
            "text": "不自动",
            "value": false
          }
        ],
        "inCommonUse": true,
        "desc": "<div>自动计算：数据中最大的数值作为100%,其余自动计算</div><div style='margin-top:6px;'>不自动计算时：数据直接作为百分比数据展示</div>"
      },
      {
        "text": "排序",
        "showType": "dv_radio_group",
        "bindControlAttr": "sortInfo",
        "dictionary": [
          {
            "text": "默认",
            "value": "default"
          },
          {
            "text": "升序",
            "value": "asc"
          },
          {
            "text": "降序",
            "value": "desc"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "规则样式",
        "bindControlAttr": "colorRuleList",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDVUniversalRuleConfig",
          "width": "1000px",
          "height": "600px",
          "typeOptions": [
            {
              "label": "文字",
              "value": "text"
            },
            {
              "label": "色值",
              "value": "color",
              "config": {
                "type": "color"
              }
            }
          ]
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "颜色",
        "bindControlAttr": "backgroundConfig",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "backgroundConfig",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "page"
          ]
        }
      },
      {
        "text": "外阴影",
        "bindControlAttr": "boxOutsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset"
        },
        "inCommonUse": true
      },
      {
        "text": "内阴影",
        "bindControlAttr": "boxInsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "inset"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "滚动设置",
    "attributes": [
      {
        "text": "自动滚动",
        "showType": "dv_switch",
        "bindControlAttr": "isScroll",
        "inCommonUse": true
      },
      {
        "text": "滚动速度",
        "showType": "dv_number",
        "showExpress": "@[isScroll === true]",
        "bindControlAttr": "scrollSpeed",
        "spacingNumber": 1,
        "inCommonUse": true,
        "config": {
          "min": 0.1,
          "precision": 1,
          "unit": "px"
        }
      }
    ]
  },
  {
    "name": "属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "懒加载",
        "showType": "dv_switch",
        "bindControlAttr": "isHandlerRefresh"
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]