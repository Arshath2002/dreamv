[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "height",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "wholePadding"
            },
            {
              "text": "内容内边距",
              "bindControlAttr": "padding"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "边框设置",
    "attributes": [
      {
        "text": "",
        "bindControlAttr": "borderConfig",
        "showType": "dv_border",
        "inCommonUse": true
      },
      {
        "text": "圆角",
        "showType": "dv_border_radius",
        "showExpress": "@[borderConfig.border || borderConfig.borderTop || borderConfig.borderLeft || borderConfig.borderRight || borderConfig.borderBottom]",
        "spacingNumber": -1,
        "bindControlAttr": "borderRadius",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "颜色",
        "bindControlAttr": "backgroundConfig",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "backgroundConfig",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1,
          "backgroundTypes":["page","model","nav","title","board","boardIcon"]
        }
      },
      {
        "text": "外阴影",
        "bindControlAttr": "boxOutsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset"
        },
        "inCommonUse": true
      },
      {
        "text": "内阴影",
        "bindControlAttr": "boxInsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "inset"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "波纹动效",
    "attributes": [
      {
        "text": "波纹动效",
        "bindControlAttr": "waveSetting.show",
        "showExpress": "@[backgroundConfig[backgroundConfig.active].image]",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "波纹设置",
        "bindControlAttr": "waveSetting",
        "showExpress": "@[waveSetting.show]",
        "showType": "dv_dialog",
        "inCommonUse": true,
        "config": {
          "title": "波纹设置-按住鼠标左键滑动产生波纹",
          "content": "DesDVPaintImage",
          "width": "1000px",
          "height": "600px",
          "isClear": true,
          "clearClose": false,
          "valueExpress": "@[maskImage]"
        }
      }
    ]
  },
  {
    "name": "属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[click.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]