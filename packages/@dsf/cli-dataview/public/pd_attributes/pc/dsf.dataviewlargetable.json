        "inCommonUse": true,
        "config": {
          "hMapData": [
            {
              "mapName": "flex-start",
              "mapKey": "left"
            },
            {
              "mapName": "center",
              "mapKey": "center"
            },
            {
              "mapName": "flex-end",
              "mapKey": "right"
            }
          ],
          "vMapData": [
            {
              "mapName": "flex-start",
              "mapKey": "top"
            },
            {
              "mapName": "center",
              "mapKey": "middle"
            },
            {
              "mapName": "flex-end",
              "mapKey": "bottom"
            }
          ]
        }
      },
      {
        "text": "自定义内容",
        "bindControlAttr": "pageConfig.btnList",
        "showExpress": "@[pageConfig.isPage && pageConfig.selectStyle === 'default']",
        "showType": "dv_dialog",
        "hiddenText": true,
        "inCommonUse": true,
        "config": {
          "content": "DesDVPageCustom",
          "width": "600px",
          "height": "300px",
          "showBtnType": "common"
        }
      }
    ]
  },
  {
    "name": "滚动设置",
    "attributes": [
      {
        "text": "循环滚动",
        "showType": "dv_switch",
        "showExpress": "@[!isVirtualTable && !isScroll]",
        "bindControlAttr": "isLoopScroll",
        "inCommonUse": true
      },
      {
        "text": "虚拟滚动",
        "showType": "dv_switch",
        "bindControlAttr": "isVirtualTable",
        "inCommonUse": true
      },
      {
        "text": "自动滚动",
        "showType": "dv_switch",
        "bindControlAttr": "isScroll",
        "inCommonUse": true
      },
      {
        "text": "滚动速度",
        "showType": "dv_number",
        "showExpress": "@[isScroll === true]",
        "bindControlAttr": "scrollSpeed",
        "spacingNumber": 1,
        "inCommonUse": true,
        "config": {
          "min": 0.1,
          "precision": 1,
          "unit": "px"
        }
      },
      {
        "text": "悬浮暂停",
        "showType": "dv_switch",
        "showExpress": "@[isScroll === true]",
        "bindControlAttr": "scrollHoverPause",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "showType": "dv_line",
        "spacingNumber": 1,
        "showExpress": "@[scrollStyleConfig.isShowHScroll]",
        "inCommonUse": true
      },
      {
        "text": "自定义横向滚动条样式",
        "showType": "dv_switch",
        "showExpress": "@[scrollStyleConfig.isShowHScroll]",
        "bindControlAttr": "scrollStyleConfig.isCustomStyle",
        "inCommonUse": true
      },
