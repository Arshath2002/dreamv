        "dialogType": "relation",
        "bindControlAttr": "headData",
        "showType": "dv_dialog",
        "inCommonUse": true,
        "config": {
          "content": "DesDVTableEdit",
          "width": "1100px",
          "height": "60vh",
          "params": {
            "componentType": "largeTable"
          },
          "showBtnType": "common"
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "showType": "dv_radio_group",
        "bindControlAttr": "propStyleFakeSelect",
        "dictionary": [
          {
            "text": "整体",
            "value": "all"
          },
          {
            "text": "单列",
            "value": "single"
          }
        ],
        "config": {
          "autoFit": true,
          "stroke": true
        },
        "inCommonUse": true
      },
      {
        "text": "",
        "hiddenText": true,
        "showType": "dv_single_table_style",
        "showExpress": "@[propStyleFakeSelect === 'single']",
        "bindControlAttr": "headData",
        "config": {
          "componentType": "largeTable"
        },
        "inCommonUse": true
      },
      {
        "text": "表头",
        "showType": "dv_switch",
        "showExpress": "@[propStyleFakeSelect === 'all']",
        "bindControlAttr": "newHeaderConfig.isShowHeader",
        "inCommonUse": true
      },
      {
        "text": "高",
