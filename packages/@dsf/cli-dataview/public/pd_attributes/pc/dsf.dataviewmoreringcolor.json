[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "宽",
        "showType": "dv_pixel",
        "bindControlAttr": "width",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "showType": "dv_pixel",
        "bindControlAttr": "height",
        "itemClass": "float-one-row",
        "spacingNumber": 1,
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "映射字段",
        "bindControlAttr": "mapRelations",
        "showType": "dv_dialog",
        "dialogType":"relation",
        "inCommonUse": true,
        "config": {
          "title": "设置",
          "content": "DesDVMapRelationSetting",
          "width": "1100px",
          "height": "60vh",
          "showBtnType": "common"
        },
        "textCss": {
          "display": "none"
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "对齐方式",
        "showType": "dv_text_align",
        "bindControlAttr": "flex",
        "config": {
          "isShowVAlign": false,
          "hMapData": [
            {
              "mapName": "flex-start",
              "mapKey": "left"
            },
            {
              "mapName": "center",
              "mapKey": "center"
            },
            {
              "mapName": "flex-end",
              "mapKey": "right"
            }
          ]
        },
        "inCommonUse": true
      },
      {
        "text": "大小类型",
        "showType": "dv_radio_group",
        "bindControlAttr": "sizeMode",
        "dictionary": [
          {
            "text": "固定大小",
            "value": "px"
          },
          {
            "text": "一行个数",
            "value": "num"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "一行个数",
        "bindControlAttr": "rowNum",
        "showExpress": "@[sizeMode==='num']",
        "showType": "dv_number",
        "config": {
          "min": 1
        },
        "inCommonUse": true
      },
      {
        "text": "圆环大小",
        "bindControlAttr": "size",
        "showExpress": "@[sizeMode==='px']",
        "showType": "dv_number",
        "config": {
          "min": 120
        },
        "inCommonUse": true
      },
      {
        "text": "标题样式",
        "showType": "dv_text_style",
        "bindControlAttr": "titleTextStyle",
        "inCommonUse": true
      },
      {
        "text": "内容样式",
        "showType": "dv_text_style",
        "bindControlAttr": "valueTextStyle",
        "inCommonUse": true
      },
      {
        "text": "圆环边距",
        "showType": "dv_margins",
        "bindControlAttr": "barPadding",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "圆环属性",
    "attributes": [
      {
        "text": "圆环颜色",
        "bindControlAttr": "config.bigCircleStrokeArr",
        "showType": "dv_svg_ring",
        "inCommonUse": true
      },
      {
        "text": "节点颜色",
        "bindControlAttr": "config.nodeCircleStrokeArr",
        "showType": "dv_more_color",
        "inCommonUse": true
      },
      {
        "text": "竖条数量",
        "bindControlAttr": "config.rectNum",
        "showType": "dv_number",
        "config": {
          "min": 10
        },
        "inCommonUse": true
      },
      {
        "text": "竖条宽度",
        "bindControlAttr": "config.rectWidth",
        "showType": "dv_number",
        "itemClass": "float-one-row",
        "config": {
          "min": 8
        },
        "inCommonUse": true
      },
      {
        "text": "高度",
        "bindControlAttr": "config.rectHeight",
        "showType": "dv_number",
        "itemClass": "float-one-row ml20",
        "config": {
          "min": 8
        },
        "inCommonUse": true
      },
      {
        "text": "竖条颜色",
        "bindControlAttr": "config.linesColors",
        "showType": "dv_more_color",
        "inCommonUse": true,
        "config": {
          "type": "linear"
        }
      },
      {
        "text": "渐变方向",
        "showType": "dv_radio_group",
        "bindControlAttr": "config.gradientDirection",
        "dictionary": [
          {
            "text": "垂直",
            "value": "vec"
          },
          {
            "text": "环形",
            "value": "circle"
          }
        ],
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        },
        "inCommonUse": true
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      },
      {
        "text": "懒加载",
        "showType": "dv_switch",
        "bindControlAttr": "isHandlerRefresh"
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]