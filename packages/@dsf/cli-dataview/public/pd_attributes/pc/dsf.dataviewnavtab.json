[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "height",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "选项卡设置",
        "hiddenText": true,
        "textCss": {
          "min-width": 0
        },
        "showType": "dv_dialog",
        "bindControlAttr": "slots",
        "changeExecFunction": "changeActive",
        "config": {
          "content": "DesDVNavTabEdit",
          "width": "1000px",
          "height": "60vh",
          "buttonText": "设置",
          "title": "选项卡设置",
          "hasBadge": true,
          "showBtnType": "common"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "隐藏页签",
        "bindControlAttr": "isHideHeader",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "页签宽度",
        "bindControlAttr": "navTabWidthType",
        "showExpress": "@[!isHideHeader]",
        "showType": "dv_radio_group",
        "itemClass": "float-one-row",
        "spacingNumber": 1,
        "dictionary": [
          {
            "text": "自适应",
            "value": "auto"
          },
          {
            "text": "固定",
            "value": "fixed"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "页签宽度",
        "hiddenText": true,
        "textCss": {
          "min-width": 0
        },
        "bindControlAttr": "controlNavTabWidth",
        "showExpress": "@[!isHideHeader && navTabWidthType === 'fixed']",
        "itemClass": "float-one-row",
        "showType": "dv_pixel",
        "inCommonUse": true
      },
      {
        "text": "纵向位置",
        "bindControlAttr": "tabPosition",
        "spacingNumber": 1,
        "showType": "dv_radio_group",
        "showExpress": "@[!isHideHeader]",
        "dictionary": [
          {
            "text": "顶部",
            "value": "top"
          },
          {
            "text": "左侧",
            "value": "left"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "横向位置",
        "bindControlAttr": "navPosition",
        "showExpress": "@[!isHideHeader]",
        "showType": "dv_radio_group",
        "spacingNumber": 1,
        "dictionary": [
          {
            "text": "开始",
            "value": "start"
          },
          {
            "text": "居中",
            "value": "center"
          },
          {
            "text": "结束",
            "value": "end"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "下边框颜色",
        "bindControlAttr": "navBorder.color",
        "showType": "dv_color",
        "spacingNumber": 1,
        "showExpress": "@[!isHideHeader]",
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "",
        "hiddenText": true,
        "showType": "dv_radio_group",
        "bindControlAttr": "selectOrNoType",
        "showExpress": "@[!isHideHeader]",
        "dictionary": [
          {
            "text": "样式",
            "value": "default"
          },
          {
            "text": "选中样式",
            "value": "select"
          }
        ],
        "config": {
          "autoFit": true,
          "stroke": true
        },
        "inCommonUse": true
      },
      {
        "text": "样式",
        "bindControlAttr": "noSelectStyle",
        "showType": "dv_text_style",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'default' ]",
        "config": {
          "picker": {
            "type": "color"
          }
        },
        "inCommonUse": true
      },
      {
        "text": "背景颜色",
        "bindControlAttr": "noSelectStyle.bgConfig",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'default' ]",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "背景图片",
        "bindControlAttr": "noSelectStyle.bgConfig",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'default' ]",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "nav"
          ]
        }
      },
      {
        "text": "阴影色",
        "bindControlAttr": "noSelectStyle.shadowColor",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'default' ]",
        "showType": "dv_color",
        "config": {
          "picker": {
            "type": "color"
          }
        },
        "inCommonUse": true
      },
      {
        "text": "边框",
        "bindControlAttr": "noSelectStyle.borderConfig",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'default' ]",
        "showType": "dv_border",
        "inCommonUse": true,
        "config": {
          "isShowTop": false,
          "isShowRight": false,
          "isShowBottom": false,
          "isShowLeft": false
        }
      },
      {
        "text": "圆角",
        "showType": "dv_border_radius",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'default' && noSelectStyle.borderConfig.border ]",
        "bindControlAttr": "borderRadius",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'default' ]",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "noSelectStyle.padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "noSelectStyle.margin"
            }
          ]
        }
      },
      {
        "showType": "dv_line",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'default' ]",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "样式",
        "bindControlAttr": "selectStyle",
        "showType": "dv_text_style",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'select' ]",
        "config": {
          "fontSizeField": "$fontSize",
          "picker": {
            "type": "color"
          }
        },
        "inCommonUse": true
      },
      {
        "text": "背景颜色",
        "bindControlAttr": "selectStyle.bgConfig",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'select' ]",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "背景图片",
        "bindControlAttr": "selectStyle.bgConfig",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'select' ]",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "nav"
          ]
        }
      },
      {
        "text": "阴影色",
        "bindControlAttr": "selectStyle.shadowColor",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'select' ]",
        "showType": "dv_color",
        "config": {
          "picker": {
            "type": "color"
          }
        },
        "inCommonUse": true
      },
      {
        "text": "边框",
        "bindControlAttr": "selectStyle.borderConfig",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'select' ]",
        "showType": "dv_border",
        "inCommonUse": true,
        "config": {
          "isShowTop": false,
          "isShowRight": false,
          "isShowBottom": false,
          "isShowLeft": false
        }
      },
      {
        "text": "圆角",
        "showType": "dv_border_radius",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'select' && selectStyle.borderConfig.border  ]",
        "bindControlAttr": "selectStyle.$borderRadius",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'select' ]",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "selectStyle.padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "selectStyle.margin"
            }
          ]
        }
      },
      {
        "showType": "dv_line",
        "showExpress": "@[!isHideHeader && selectOrNoType === 'select' ]",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "内容超出",
        "bindControlAttr": "contentFit",
        "showType": "dv_radio_group",
        "dictionary": [
          {
            "text": "隐藏",
            "value": "dv_hidden"
          },
          {
            "text": "全显",
            "value": "dv_auto"
          }
        ],
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "颜色",
        "bindControlAttr": "planStyle",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "planStyle",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "model",
            "title"
          ]
        }
      },
      {
        "text": "外阴影",
        "bindControlAttr": "boxOutsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset"
        },
        "inCommonUse": true
      },
      {
        "text": "内阴影",
        "bindControlAttr": "boxInsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "inset"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      }
    ]
  },
  {
    "name": "插槽",
    "attributes": [
      {
        "text": "左上插槽",
        "isFirstText": true,
        "bindControlAttr": "isSlotTopLeft",
        "showType": "dv_switch"
      },
      {
        "text": "宽",
        "bindControlAttr": "slotTopLeftWidth",
        "showExpress": "@[isSlotTopLeft == true]",
        "itemClass": "float-one-row",
        "showType": "dv_pixel",
        "spacingNumber": 1
      },
      {
        "text": "高",
        "bindControlAttr": "slotTopLeftHeight",
        "showExpress": "@[isSlotTopLeft == true]",
        "itemClass": "float-one-row ml20",
        "showType": "dv_pixel",
        "spacingNumber": 1
      },
      {
        "text": "右上插槽",
        "isFirstText": true,
        "bindControlAttr": "isSlotTopRight",
        "showType": "dv_switch"
      },
      {
        "text": "宽",
        "bindControlAttr": "slotTopRightWidth",
        "showExpress": "@[isSlotTopRight == true]",
        "itemClass": "float-one-row",
        "showType": "dv_pixel",
        "spacingNumber": 1
      },
      {
        "text": "高",
        "bindControlAttr": "slotTopRightHeight",
        "showExpress": "@[isSlotTopRight == true]",
        "itemClass": "float-one-row ml20",
        "showType": "dv_pixel",
        "spacingNumber": 1
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]