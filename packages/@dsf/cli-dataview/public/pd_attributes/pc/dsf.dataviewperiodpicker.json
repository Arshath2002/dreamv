          {
            "text": "横向",
            "value": "row"
          },
          {
            "text": "竖向",
            "value": "column"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "副标题外边距",
        "showType": "multipleText",
        "bindControlAttr": "Subheadingmargin",
        "config": {
          "inputs": [
            {
              "placeholder": "上"
            },
            {
              "placeholder": "右"
            },
            {
              "placeholder": "下"
            },
            {
              "placeholder": "左"
            }
          ]
        },
        "inCommonUse": true
      },
      {
