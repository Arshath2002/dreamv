          {
            "text": "中",
            "value": "center"
          },
          {
            "text": "右",
            "value": "flex-end"
          },
          {
            "text": "一左一右",
            "value": "space-between"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "垂直对齐方式",
        "bindControlAttr": "vAlign",
        "showType": "select",
        "dictionary": [
          {
            "text": "上",
