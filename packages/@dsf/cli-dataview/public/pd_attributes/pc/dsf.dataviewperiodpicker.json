[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "宽度",
        "bindControlAttr": "width",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "日期选择器类型",
        "showType": "select",
        "bindControlAttr": "chooseType",
        "dictionary": [
          {
            "value": "SS",
            "text": "季度"
          },
          {
            "value": "HY",
            "text": "半年"
          },
          {
            "value": "MD",
            "text": "旬"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "高度",
        "bindControlAttr": "height",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "左插槽宽度",
        "bindControlAttr": "slotLeftWidth",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "右插槽宽度",
        "bindControlAttr": "slotRightWidth",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "背景",
        "bindControlAttr": "backgroundConfig",
        "inCommonUse": true,
        "showType": "dv_dialog",
        "config": {
          "width": "400px",
          "height": "400px",
          "content": "BackgroundSetting",
          "title": "背景设置",
          "isClear": true
        }
      },
      {
        "text": "主标题内容",
        "bindControlAttr": "text",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "主标题字号",
        "bindControlAttr": "fontSize",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "主标题字体颜色",
        "showType": "color",
        "bindControlAttr": "color",
        "inCommonUse": true
      },
      {
        "text": "副标题内容",
        "bindControlAttr": "SubheadingText",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "副标题字号",
        "bindControlAttr": "SubheadingFontSize",
