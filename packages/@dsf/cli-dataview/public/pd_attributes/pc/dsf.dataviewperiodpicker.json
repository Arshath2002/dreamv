            {
              "placeholder": "右"
            },
            {
              "placeholder": "下"
            },
            {
              "placeholder": "左"
            }
          ]
        },
        "inCommonUse": true
      },
      {
        "text": "内边距",
        "showType": "multipleText",
        "bindControlAttr": "padding",
        "config": {
          "inputs": [
            {
              "placeholder": "上"
            },
            {
              "placeholder": "右"
            },
            {
              "placeholder": "下"
            },
            {
              "placeholder": "左"
            }
          ]
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      }
    ],
    "inCommonUse": true
  }
]
