[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "showType": "dv_pixel",
        "bindControlAttr": "width",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "showType": "dv_pixel",
        "bindControlAttr": "height",
        "itemClass": "float-one-row",
        "spacingNumber": 1,
        "showExpress": "@[barType != 'bar']",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "映射字段",
        "showType": "dv_dialog",
        "bindControlAttr": "mapRelations",
        "dialogType":"relation",
        "inCommonUse": true,
        "config": {
          "title": "设置",
          "content": "DesDVMapRelationSetting",
          "width": "1100px",
          "height": "60vh",
          "showBtnType": "common"
        },
        "textCss": {
          "display": "none"
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "类型",
        "bindControlAttr": "barType",
        "showType": "dv_radio_group",
        "dictionary": [
          {
            "text": "圆环",
            "value": "circle"
          },
          {
            "text": "半圆环",
            "value": "semicircle"
          },
          {
            "text": "条形",
            "value": "line"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "标题",
        "bindControlAttr": "textTitle",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "标题样式",
        "showType": "dv_text_style",
        "bindControlAttr": "titleTextStyle",
        "inCommonUse": true,
        "config": {
          "picker": {
            "type": "color"
          }
        },
        "hiddenText": true
      },
      {
        "text": "子标题",
        "bindControlAttr": "textSubTitle",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "子标题样式",
        "showType": "dv_text_style",
        "bindControlAttr": "subTitleTextStyle",
        "inCommonUse": true,
        "config": {
          "picker": {
            "type": "color"
          }
        },
        "hiddenText": true
      },
      {
        "text": "标题间距",
        "bindControlAttr": "gap",
        "showType": "dv_pixel",
        "inCommonUse": true
      },
      {
        "text": "进度条宽度",
        "bindControlAttr": "deep",
        "showType": "dv_number",
        "inCommonUse": true,
        "config": {
          "min": 1
        }
      },
      {
        "text": "主色系",
        "bindControlAttr": "mainColor",
        "showType": "dv_color",
        "inCommonUse": true,
        "config": {
          "type": "color"
        }
      },
      {
        "text": "副色系",
        "bindControlAttr": "viceColor",
        "showType": "dv_color",
        "inCommonUse": true,
        "config": {
          "type": "color"
        }
      },
      {
        "text": "进度条底色",
        "bindControlAttr": "roundColor",
        "showType": "dv_color",
        "inCommonUse": true,
        "config": {
          "type": "color"
        }
      },
      {
        "text": "开启光点",
        "bindControlAttr": "hasPointer",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "光点颜色",
        "bindControlAttr": "pointerColor",
        "showType": "dv_color",
        "showExpress": "@[hasPointer]",
        "spacingNumber": 1,
        "inCommonUse": true,
        "config": {
          "type": "color"
        }
      },
      {
        "text": "进度条动效",
        "bindControlAttr": "isAnimation",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "动效时长",
        "bindControlAttr": "animationDuration",
        "showExpress": "@[isAnimation]",
        "showType": "dv_number",
        "spacingNumber": 1,
        "inCommonUse": true,
        "config": {
          "unit": "ms"
        }
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "整体背景",
        "inCommonUse": true
      },
      {
        "text": "颜色",
        "bindControlAttr": "totalBackgroundConfig",
        "spacingNumber": 1,
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "totalBackgroundConfig",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "spacingNumber": 1,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "page"
          ]
        }
      },
      {
        "text": "外阴影",
        "bindControlAttr": "boxOutsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset"
        },
        "inCommonUse": true
      },
      {
        "text": "内阴影",
        "bindControlAttr": "boxInsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "inset"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static']",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      },
      {
        "text": "懒加载",
        "showType": "dv_switch",
        "bindControlAttr": "isHandlerRefresh"
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]