[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "高",
        "bindControlAttr": "inputHeight",
        "showType": "dv_pixel",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "选项设置",
        "bindControlAttr": "itemSet",
        "showType": "dv_dialog",
        "hiddenText": true,
        "textCss": {
          "min-width": 0
        },
        "config": {
          "width": "1000px",
          "content": "DesEnumEditor",
          "buttonText": "编辑选项",
          "title": "编辑选项",
          "showBtnType": "common"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "样式设置",
    "attributes": [
      {
        "text": "名称",
        "bindControlAttr": "nameTextStyle.isShow",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "内容",
        "showType": "dv_text",
        "showExpress": "@[nameTextStyle.isShow]",
        "bindControlAttr": "nameTextStyle.content",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "文字样式",
        "showExpress": "@[nameTextStyle.isShow]",
        "bindControlAttr": "nameTextStyle",
        "showType": "dv_text_style",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "是否多选",
        "showType": "dv_radio_group",
        "bindControlAttr": "multiple",
        "dictionary": [
          {
            "text": "单选",
            "value": false
          },
          {
            "text": "多选",
            "value": true
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "样式类型",
        "bindControlAttr": "barType",
        "showType": "dv_radio_group",
        "showExpress": "@[typeSwitch == '1' ]",
        "dictionary": [
          {
            "text": "横排",
            "value": "1"
          },
          {
            "text": "竖排",
            "value": "2"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "默认选中",
        "desc": "多选时用,号隔开。例如1,2",
        "bindControlAttr": "defaultValue",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "字体样式",
        "showType": "dv_text_style",
        "bindControlAttr": "textStyle",
        "inCommonUse": true
      },
      {
        "text": "选中颜色",
        "showType": "dv_color",
        "bindControlAttr": "textActColor",
        "inCommonUse": true
      },
      {
        "text": "选择框",
        "bindControlAttr": "typeSwitch",
        "showType": "dv_radio_group",
        "dictionary": [
          {
            "text": "展示",
            "value": "1"
          },
          {
            "text": "不展示",
            "value": "2"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "designSwitch",
        "showType": "dv_radio_group",
        "config": {
          "autoFit": true,
          "stroke": true
        },
        "dictionary": [
          {
            "text": "默认样式",
            "value": "1"
          },
          {
            "text": "选中样式",
            "value": "2"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "边框色",
        "bindControlAttr": "borderColor",
        "showType": "dv_color",
        "showExpress": "@[typeSwitch === '1'&&designSwitch==='1' ]",
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "背景色",
        "bindControlAttr": "bgColor",
        "showType": "dv_color",
        "showExpress": "@[typeSwitch === '1'&&designSwitch==='1' ]",
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "选中边框色",
        "bindControlAttr": "borderActColor",
        "showType": "dv_color",
        "showExpress": "@[typeSwitch === '1'&&designSwitch==='2' ]",
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "选中背景色",
        "bindControlAttr": "bgActColor",
        "showType": "dv_color",
        "showExpress": "@[typeSwitch === '1'&&designSwitch==='2' ]",
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "外边距",
        "bindControlAttr": "itemMargin",
        "desc": "",
        "showType": "dv_margins",
        "showExpress": "@[typeSwitch == '2' ]",
        "inCommonUse": true
      },
      {
        "text": "内边距",
        "bindControlAttr": "itemPadding",
        "desc": "",
        "showType": "dv_margins",
        "showExpress": "@[typeSwitch == '2' ]",
        "inCommonUse": true
      },
      {
        "text": "宽度",
        "bindControlAttr": "itemWidth",
        "desc": "",
        "showType": "dv_pixel",
        "showExpress": "@[typeSwitch == '2' ]",
        "inCommonUse": true
      },
      {
        "text": "选项圆角",
        "bindControlAttr": "itemRadius",
        "desc": "",
        "showType": "dv_border_radius",
        "showExpress": "@[typeSwitch == '2' ]",
        "inCommonUse": true
      },
      {
        "text": "颜色",
        "bindControlAttr": "defaultBackgroundConfig",
        "showExpress": "@[typeSwitch == '2' &&designSwitch==='1']",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "defaultBackgroundConfig",
        "showExpress": "@[typeSwitch == '2' &&designSwitch==='1']",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1
        }
      },
      {
        "text": "颜色",
        "bindControlAttr": "clickBackgroundConfig",
        "showExpress": "@[typeSwitch == '2' &&designSwitch==='2']",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "clickBackgroundConfig",
        "showExpress": "@[typeSwitch == '2' &&designSwitch==='2']",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1
        }
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "颜色",
        "bindControlAttr": "controlBackgroundConfig",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "controlBackgroundConfig",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "page"
          ]
        }
      },
      {
        "text": "外阴影",
        "bindControlAttr": "boxOutsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset"
        },
        "inCommonUse": true
      },
      {
        "text": "内阴影",
        "bindControlAttr": "boxInsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "inset"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "业务组件",
        "showType": "dv_dialog",
        "bindControlAttr": "businessComponents",
        "config": {
          "title": "业务组件选择",
          "content": "DesDVBusinessComponents",
          "width": "1000px",
          "isObjectValue": true,
          "multiple": true,
          "height": "60vh",
          "isClear": true,
          "clearClose": false,
          "buttonText": "设置"
        }
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]