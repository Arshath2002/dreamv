[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "height",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "映射字段",
        "showType": "dv_dialog",
        "bindControlAttr": "mapRelations",
        "dialogType":"relation",
        "inCommonUse": true,
        "config": {
          "title": "设置",
          "content": "DesDVMapRelationSetting",
          "width": "1100px",
          "height": "60vh",
          "showBtnType": "common"
        },
        "textCss": {
          "display": "none"
        }
      }
    ]
  },
  {
    "name": "组件设置",
    "attributes": [
      {
        "text": "名称",
        "showType": "dv_switch",
        "bindControlAttr": "titleFlag",
        "inCommonUse": true
      },
      {
        "text": "名称",
        "hiddenText": true,
        "showType": "dv_text_style",
        "showExpress": "@[titleFlag]",
        "bindControlAttr": "titleTextStyle",
        "inCommonUse": true
      },
      {
        "text": "展示内容",
        "showType": "dv_switch",
        "bindControlAttr": "valueFlag",
        "inCommonUse": true
      },
      {
        "text": "数值格式",
        "showType": "dv_text",
        "showExpress": "@[valueFlag]",
        "bindControlAttr": "valueFormatter",
        "inCommonUse": true
      },
      {
        "text": "展示内容",
        "hiddenText": true,
        "showType": "dv_text_style",
        "showExpress": "@[valueFlag]",
        "bindControlAttr": "valueTextStyle",
        "inCommonUse": true
      },
      {
        "text": "条形颜色",
        "bindControlAttr": "colors",
        "showType": "dv_more_color",
        "inCommonUse": true,
        "config": {
          "type": "both"
        }
      },
      {
        "text": "条形动效",
        "bindControlAttr": "animateConfig.flag",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "时长",
        "showType": "dv_number",
        "showExpress": "@[animateConfig.flag]",
        "bindControlAttr": "animateConfig.dur",
        "config": {
          "unit": "ms"
        },
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "底色",
        "showType": "dv_color",
        "bindControlAttr": "barBackground",
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "高度",
        "showType": "dv_pixel",
        "bindControlAttr": "barHeight",
        "inCommonUse": true
      },
      {
        "text": "圆角",
        "showType": "dv_border_radius",
        "bindControlAttr": "borderRadius",
        "inCommonUse": true
      },
      {
        "text": "计算%",
        "bindControlAttr": "autoCalculate",
        "showType": "dv_switch",
        "inCommonUse": true,
        "desc": "<div>自动计算：数据中最大的数值作为100%,其余自动计算</div><div style='margin-top:6px;'>不自动计算时：数据直接作为百分比数据展示</div>"
      },
      {
        "text": "排序",
        "showType": "dv_radio_group",
        "bindControlAttr": "sortInfo",
        "dictionary": [
          {
            "text": "默认",
            "value": "default"
          },
          {
            "text": "升序",
            "value": "asc"
          },
          {
            "text": "降序",
            "value": "desc"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "展示行高",
        "showType": "dv_radio_group",
        "itemClass": "float-one-row",
        "bindControlAttr": "fixEachRowType",
        "dictionary": [
          {
            "text": "行数",
            "value": "rowNum"
          },
          {
            "text": "高度",
            "value": "fixHeight"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "一屏行数",
        "hiddenText": true,
        "showExpress": "@[fixEachRowType === 'rowNum']",
        "bindControlAttr": "rowNum",
        "showType": "dv_number",
        "textCss": {
          "min-width": 0
        },
        "itemClass": "float-one-row",
        "inCommonUse": true,
        "config": {
          "min": 1,
          "unit": "行"
        }
      },
      {
        "text": "每行高度",
        "showType": "dv_pixel",
        "hiddenText": true,
        "textCss": {
          "min-width": 0
        },
        "showExpress": "@[fixEachRowType === 'fixHeight']",
        "itemClass": "float-one-row",
        "bindControlAttr": "rowHeight",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "颜色",
        "bindControlAttr": "background",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "background",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "page"
          ]
        }
      },
      {
        "text": "外阴影",
        "bindControlAttr": "boxOutsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset"
        },
        "inCommonUse": true
      },
      {
        "text": "内阴影",
        "bindControlAttr": "boxInsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "inset"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "滚动设置",
    "attributes": [
      {
        "text": "自动滚动",
        "showType": "dv_switch",
        "bindControlAttr": "isScroll",
        "inCommonUse": true
      },
      {
        "text": "滚动速度",
        "showExpress": "@[isScroll === true]",
        "spacingNumber": 1,
        "bindControlAttr": "scrollSpeed",
        "inCommonUse": true,
        "showType": "dv_number",
        "desc": "从0.1开始值越大越快",
        "config": {
          "min": 0.1,
          "precision": 1
        }
      }
    ]
  },
  {
    "name": "属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "懒加载",
        "showType": "dv_switch",
        "bindControlAttr": "isHandlerRefresh"
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]