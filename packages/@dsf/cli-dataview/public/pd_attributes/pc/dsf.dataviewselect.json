[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "selectWidth",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "inputHeight",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "选项设置",
        "bindControlAttr": "itemSet",
        "showType": "dv_dialog",
        "hiddenText": true,
        "textCss": {
          "min-width": 0
        },
        "config": {
          "width": "1000px",
          "content": "DesEnumEditor",
          "buttonText": "编辑选项",
          "title": "编辑选项",
          "showBtnType": "common"
        },
        "inCommonUse": true
      },
      {
        "text": "",
        "bindControlAttr": "items",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "样式设置",
    "attributes": [
      {
        "text": "整体",
        "showType": "dv_text_align",
        "bindControlAttr": "align",
        "inCommonUse": true,
        "config": {
          "isShowVAlign": false
        }
      },
      {
        "text": "名称",
        "bindControlAttr": "nameTextStyle.isShow",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "内容",
        "showType": "dv_text",
        "showExpress": "@[nameTextStyle.isShow]",
        "bindControlAttr": "nameTextStyle.content",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "文字样式",
        "showExpress": "@[nameTextStyle.isShow]",
        "bindControlAttr": "nameTextStyle",
        "showType": "dv_text_style",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "提示文字",
        "bindControlAttr": "placeholder",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "是否多选",
        "showType": "dv_radio_group",
        "bindControlAttr": "multiple",
        "dictionary": [
          {
            "text": "单选",
            "value": false
          },
          {
            "text": "多选",
            "value": true
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "文字样式",
        "bindControlAttr": "selectTextStyle",
        "showType": "dv_text_style",
        "showExpress": "@[relationStyle]",
        "config": {
          "isShowColor": false,
          "isShowItalics": false,
          "isShowUnderline": false,
          "isShowFontWeight": false,
          "isShowShadow": false
        },
        "inCommonUse": true
      },
      {
        "text": "依赖风格",
        "bindControlAttr": "relationStyle",
        "showType": "dv_switch",
        "desc": "依赖-部分样式无法设置，将会根据风格设定",
        "inCommonUse": true
      },
      {
        "text": "边框圆角",
        "showType": "dv_border_radius",
        "showExpress": "@[borderConfig.border || borderConfig.borderTop || borderConfig.borderLeft || borderConfig.borderRight || borderConfig.borderBottom]",
        "bindControlAttr": "borderRadius",
        "inCommonUse": true
      },
      {
        "text": "文字样式",
        "bindControlAttr": "selectTextStyle",
        "showType": "dv_text_style",
        "showExpress": "@[!relationStyle]",
        "config": {
          "isShowItalics": false,
          "isShowUnderline": false,
          "isShowFontWeight": false,
          "isShowShadow": false
        },
        "inCommonUse": true
      },
      {
        "text": "边框样式",
        "showExpress": "@[!relationStyle]",
        "inCommonUse": true
      },
      {
        "text": "",
        "bindControlAttr": "borderConfig",
        "showExpress": "@[!relationStyle]",
        "showType": "dv_border",
        "itemClass": "ml30",
        "inCommonUse": true
      },
      {
        "text": "下拉样式",
        "showExpress": "@[!relationStyle]",
        "inCommonUse": true
      },
      {
        "text": "背景色",
        "bindControlAttr": "customThemeStyle.--dv-select-dropdown-bg",
        "showType": "dv_color",
        "showExpress": "@[!relationStyle]",
        "config": {
          "type": "color"
        },
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "悬浮色",
        "bindControlAttr": "customThemeStyle.--dv-active-select-dropdown-bg",
        "showType": "dv_color",
        "showExpress": "@[!relationStyle]",
        "config": {
          "type": "color"
        },
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "边框颜色",
        "bindControlAttr": "customThemeStyle.--dv-select-dropdown-border-color",
        "showType": "dv_color",
        "showExpress": "@[!relationStyle]",
        "config": {
          "type": "color"
        },
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "滚动条颜色",
        "bindControlAttr": "customThemeStyle.--dv-select-dropdown-scrollbar-color",
        "showType": "dv_color",
        "showExpress": "@[!relationStyle]",
        "config": {
          "type": "color"
        },
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "圆角",
        "bindControlAttr": "customThemeStyle.--dv-select-dropdown-border-radius",
        "showType": "dv_border_radius",
        "showExpress": "@[!relationStyle]",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "字体颜色",
        "bindControlAttr": "customThemeStyle.--dv-select-dropdown-text-color",
        "showType": "dv_color",
        "showExpress": "@[!relationStyle]",
        "config": {
          "type": "color"
        },
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "选中颜色",
        "bindControlAttr": "customThemeStyle.--dv-active-multiple-dropdown-text-color",
        "showType": "dv_color",
        "showExpress": "@[!relationStyle]",
        "config": {
          "type": "color"
        },
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "showType": "dv_line",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "默认值",
        "bindControlAttr": "defaultValue",
        "showType": "dv_text",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "颜色",
        "bindControlAttr": "controlBackgroundConfig",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "controlBackgroundConfig",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "page"
          ]
        }
      },
      {
        "text": "外阴影",
        "bindControlAttr": "boxOutsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset"
        },
        "inCommonUse": true
      },
      {
        "text": "内阴影",
        "bindControlAttr": "boxInsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "inset"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "业务组件",
        "showType": "dv_dialog",
        "bindControlAttr": "businessComponents",
        "config": {
          "title": "业务组件选择",
          "content": "DesDVBusinessComponents",
          "width": "1000px",
          "isObjectValue": true,
          "multiple": true,
          "height": "60vh",
          "isClear": true,
          "clearClose": false,
          "buttonText": "设置"
        }
      },
      {
        "text": "是否挂载body",
        "showType": "dv_switch",
        "bindControlAttr": "popperAppendToBody"
      },
      {
        "text": "选择框类名",
        "showType": "dv_text",
        "bindControlAttr": "dropDownClass"
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]