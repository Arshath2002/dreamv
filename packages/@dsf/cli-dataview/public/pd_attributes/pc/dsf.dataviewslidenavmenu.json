[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "height",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "菜单配置",
        "hiddenText": true,
        "textCss": {
          "min-width": 0
        },
        "bindControlAttr": "staticData",
        "showType": "dv_dialog",
        "inCommonUse": true,
        "config": {
          "content": "DesDVCommonMenuEditor",
          "width": "1200px",
          "height": "60vh",
          "iconImageType": "image",
          "showBtnType": "common"
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "位置",
        "showType": "dv_text_align",
        "bindControlAttr": "navAlign",
        "config": {
          "isShowVAlign": false,
          "hMapData": [
            {
              "mapName": "flex-start",
              "mapKey": "left"
            },
            {
              "mapName": "center",
              "mapKey": "center"
            },
            {
              "mapName": "flex-end",
              "mapKey": "right"
            }
          ],
          "vMapData": [
            {
              "mapName": "flex-start",
              "mapKey": "top"
            },
            {
              "mapName": "center",
              "mapKey": "middle"
            },
            {
              "mapName": "flex-end",
              "mapKey": "bottom"
            }
          ]
        },
        "inCommonUse": true
      },
      {
        "text": "显示个数",
        "showType": "dv_number",
        "itemClass": "float-one-row",
        "bindControlAttr": "showCount",
        "config": {
          "min": 1
        },
        "inCommonUse": true
      },
      {
        "text": "弧度",
        "showType": "dv_number",
        "bindControlAttr": "itemIndentY",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "项宽",
        "showType": "dv_number",
        "itemClass": "float-one-row",
        "spacingNumber": 1,
        "bindControlAttr": "itemWidth",
        "inCommonUse": true
      },
      {
        "text": "项高",
        "showType": "dv_pixel",
        "bindControlAttr": "itemHeight",
        "itemClass": "float-one-row ml20",
        "config": {
          "options": [
            {
              "value": "%",
              "label": "%"
            },
            {
              "value": "px",
              "label": "px"
            }
          ]
        },
        "inCommonUse": true
      },
      {
        "text": "间距",
        "showType": "dv_number",
        "itemClass": "float-one-row",
        "spacingNumber": 1,
        "bindControlAttr": "itemGap",
        "inCommonUse": true
      },
      {
        "text": "字体样式",
        "showType": "dv_text_style",
        "bindControlAttr": "textStyle",
        "inCommonUse": true
      },
      {
        "text": "对齐方式",
        "hiddenText": true,
        "showType": "dv_text_align",
        "bindControlAttr": "textAlign",
        "config": {
          "isShowVAlign": false,
          "iconType": "textContent"
        },
        "inCommonUse": true
      },
      {
        "text": "图标边距",
        "showType": "dv_margins",
        "bindControlAttr": "iconPadding",
        "desc": "<p>只在导航菜单图标或图片存在时生效。<p>",
        "inCommonUse": true
      },
      {
        "text": "单个背景",
        "inCommonUse": true
      },
      {
        "text": "颜色",
        "bindControlAttr": "itemBackgroundConfig",
        "spacingNumber": 1,
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "itemBackgroundConfig",
        "inCommonUse": true,
        "showType": "dv_more_img",
        "spacingNumber": 1,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "nav"
          ]
        }
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "整体背景",
        "inCommonUse": true
      },
      {
        "text": "颜色",
        "bindControlAttr": "panelBackgroundConfig",
        "spacingNumber": 1,
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "panelBackgroundConfig",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "spacingNumber": 1,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "nav"
          ]
        }
      },
      {
        "text": "外阴影",
        "bindControlAttr": "boxOutsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset"
        },
        "inCommonUse": true
      },
      {
        "text": "内阴影",
        "bindControlAttr": "boxInsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "inset"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]