[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "showType": "dv_pixel",
        "bindControlAttr": "width",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "showType": "dv_pixel",
        "bindControlAttr": "height",
        "itemClass": "float-one-row",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "映射字段",
        "showType": "dv_dialog",
        "dialogType": "relation",
        "bindControlAttr": "",
        "inCommonUse": true,
        "config": {
          "title": "设置",
          "content": "DesDVDataSetting",
          "width": "500px",
          "height": "60vh",
          "showBtnType": "common"
        },
        "textCss": {
          "display": "none"
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "文本内容",
        "showType": "dv_text",
        "bindControlAttr": "textContent",
        "config": {
          "type": "textarea",
          "rows":4
        },
        "inCommonUse": true
      },
      {
        "text": "文本",
        "showType": "dv_text_style",
        "bindControlAttr": "titleTextStyle",
        "inCommonUse": true
      },
      {
        "text": "规则样式",
        "showType": "dv_dialog",
        "dialogType": "relation",
        "bindControlAttr": "fieldsStyleRules",
        "inCommonUse": true,
        "config": {
          "title": "字段字体样式规则",
          "content": "DesDVDataFontStyleRules",
          "width": "800px",
          "height": "520px",
          "customClass": "tool-tip-font-style-edit-wrap"
        }
      },
      {
        "text": "开启动画",
        "showType": "dv_switch",
        "bindControlAttr": "isAnimate",
        "inCommonUse": true
      },
      {
        "text": "延迟时间",
        "showType": "dv_number",
        "showExpress": "@[isAnimate]",
        "spacingNumber": 1,
        "bindControlAttr": "delayTime",
        "inCommonUse": true
      },
      
      {
        "text": "移动速度",
        "showType": "dv_number",
        "showExpress": "@[isAnimate]",
        "spacingNumber": 1,
        "bindControlAttr": "speed",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      },
      {
        "text": "懒加载",
        "showType": "dv_switch",
        "bindControlAttr": "isHandlerRefresh"
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]
