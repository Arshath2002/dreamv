[
  {
    "name": "基础属性",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "inputWidth",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "inputHeight",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "整体",
        "showType": "dv_text_align",
        "bindControlAttr": "textAlign",
        "config": {
          "isShowVAlign": false
        },
        "inCommonUse": true
      },
      {
        "text": "名称",
        "bindControlAttr": "nameTextStyle.isShow",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "内容",
        "showType": "dv_text",
        "showExpress": "@[nameTextStyle.isShow]",
        "bindControlAttr": "nameTextStyle.content",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "文字样式",
        "showExpress": "@[nameTextStyle.isShow]",
        "bindControlAttr": "nameTextStyle",
        "showType": "dv_text_style",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "提示文字",
        "bindControlAttr": "placeholder",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "依赖风格",
        "bindControlAttr": "relationStyle",
        "showType": "dv_switch",
        "desc": "依赖-部分样式无法设置，将会根据风格设定",
        "inCommonUse": true
      },
      {
        "text": "样式",
        "showType": "dv_text_style",
        "showExpress": "@[relationStyle]",
        "bindControlAttr": "contentTextStyle",
        "config": {
          "isShowColor": false,
          "isShowItalics": false,
          "isShowUnderline": false,
          "isShowFontWeight": false,
          "isShowShadow": false
        },
        "inCommonUse": true
      },
      {
        "text": "样式",
        "showType": "dv_text_style",
        "showExpress": "@[!relationStyle]",
        "bindControlAttr": "contentTextStyle",
        "config": {
          "picker": {
            "type": "color"
          },
          "isShowItalics": false,
          "isShowUnderline": false,
          "isShowFontWeight": false,
          "isShowShadow": false
        },
        "inCommonUse": true
      },
      {
        "text": "图标位置",
        "showType": "dv_radio_group",
        "bindControlAttr": "iconPosition",
        "dictionary": [
          {
            "text": "前",
            "value": "before"
          },
          {
            "text": "后",
            "value": "after"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "图标颜色",
        "showType": "dv_color",
        "bindControlAttr": "iconColor",
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "边框样式",
        "bindControlAttr": "",
        "showType": "",
        "inCommonUse": true
      },
      {
        "text": "",
        "bindControlAttr": "borderConfig",
        "showType": "dv_border",
        "itemClass": "ml30",
        "inCommonUse": true
      },
      {
        "text": "获取焦点",
        "bindControlAttr": "",
        "showType": "",
        "inCommonUse": true
      },
      {
        "text": "边框",
        "showType": "dv_color",
        "config": {
          "type": "color"
        },
        "bindControlAttr": "focusBorderColor",
        "inCommonUse": true
      },
      {
        "text": "边框圆角",
        "showExpress": "@[!relationStyle]",
        "showType": "dv_border_radius",
        "bindControlAttr": "borderRadius",
        "inCommonUse": true
      },
      {
        "text": "背景颜色",
        "showType": "dv_color",
        "config": {
          "type": "color"
        },
        "showExpress": "@[!relationStyle]",
        "bindControlAttr": "contentBgColor",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "颜色",
        "bindControlAttr": "controlBackgroundConfig",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "controlBackgroundConfig",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "page"
          ]
        }
      },
      {
        "text": "外阴影",
        "bindControlAttr": "boxOutsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset"
        },
        "inCommonUse": true
      },
      {
        "text": "内阴影",
        "bindControlAttr": "boxInsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "inset"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[search.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]