[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "height",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "数据",
    "attributes": [
      {
        "text": "映射字段",
        "showType": "dv_dialog",
        "hiddenText": true,
        "textCss": {
          "min-width": 0
        },
        "bindControlAttr": "mapRelations",
        "inCommonUse": true,
        "config": {
          "title": "映射字段设置",
          "content": "DesDVMapRelationSetting",
          "width": "1100px",
          "height": "60vh",
          "buttonText": "设置",
          "showBtnType": "common"
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "排列方式",
        "showType": "dv_radio_group",
        "bindControlAttr": "direction",
        "dictionary": [
          {
            "text": "竖向",
            "value": "vertical"
          },
          {
            "text": "横向",
            "value": "horizontal"
          }
        ],
        "inCommonUse": true
      },
      {
        "text": "时间文本",
        "showType": "dv_switch",
        "bindControlAttr": "isShowTime",
        "inCommonUse": true
      },
      {
        "text": "文本",
        "showType": "dv_text_style",
        "bindControlAttr": "timeConfigStyle",
        "showExpress": "@[isShowTime]",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "间距",
        "showType": "dv_number",
        "bindControlAttr": "timeSpace",
        "showExpress": "@[isShowTime]",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "内容文本",
        "showType": "dv_switch",
        "bindControlAttr": "isShowTextContent",
        "inCommonUse": true
      },
      {
        "text": "宽",
        "bindControlAttr": "textContentWidth",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "itemCss": {
          "margin-left": "42px"
        },
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "textContentHeight",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "textContent",
        "showType": "dv_rich_text",
        "config": {
          "title": "文本设置",
          "styleKey": "contentTextStyle"
        },
        "showExpress": "@[isShowTextContent]",
        "itemCss": {
          "padding": "0 20px 0 20px"
        },
        "inCommonUse": true
      },
      {
        "text": "样式",
        "showType": "dv_text_style",
        "bindControlAttr": "contentTextStyle",
        "showExpress": "@[isShowTextContent]",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "对齐",
        "hiddenText": true,
        "showType": "dv_text_align",
        "bindControlAttr": "contentTextAlign",
        "config": {
          "vMapData": [
            {
              "mapName": "align-flex-start",
              "mapKey": "top"
            },
            {
              "mapName": "align-center",
              "mapKey": "middle"
            },
            {
              "mapName": "align-flex-end",
              "mapKey": "bottom"
            }
          ],
          "iconType": "textContent"
        },
        "showExpress": "@[isShowTextContent]",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "内容超长策略",
        "bindControlAttr": "longStrategy",
        "showType": "dv_text",
        "desc": "<p>输入最大行数，超过将显示...，默认无，超过容器高度则出现滚动条</p>",
        "showExpress": "@[isShowTextContent]",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "鼠标悬浮查看",
        "showType": "dv_switch",
        "bindControlAttr": "isShowPopover",
        "showExpress": "@[isShowTextContent]",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "内容边距",
        "showType": "dv_margins_group",
        "showExpress": "@[isShowTextContent]",
        "inCommonUse": true,
        "spacingNumber": 1,
        "config": {
          "options": [
            {
              "text": "内容内边距",
              "bindControlAttr": "textContentPadding"
            },
            {
              "text": "内容外边距",
              "bindControlAttr": "textContentMargin"
            }
          ]
        }
      },
      {
        "text": "依赖风格",
        "bindControlAttr": "relationStyle",
        "showType": "dv_switch",
        "desc": "依赖-部分样式无法设置，将会根据风格设定",
        "inCommonUse": true
      },
      {
        "text": "内容背景",
        "bindControlAttr": "textContentBackground",
        "showExpress": "@[!relationStyle && isShowTextContent]",
        "spacingNumber": 1,
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "内容边框",
        "bindControlAttr": "textContentBorderConfig",
        "showExpress": "@[!relationStyle && isShowTextContent]",
        "spacingNumber": 1,
        "showType": "dv_border",
        "config": {
          "isShowTop": false,
          "isShowRight": false,
          "isShowBottom": false,
          "isShowLeft": false
        },
        "inCommonUse": true
      },
      {
        "text": "圆角",
        "showType": "dv_border_radius",
        "bindControlAttr": "textContentBorderRadius",
        "showExpress": "@[!relationStyle && isShowTextContent]",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "点",
        "bindControlAttr": "pointConfig.size",
        "showExpress": "@[!relationStyle]",
        "spacingNumber": 1,
        "showType": "dv_number",
        "config": {
          "min": 6
        },
        "itemClass": "float-one-row",
        "itemCss": {
          "position": "relative",
          "left": "40px"
        },
        "inCommonUse": true
      },
      {
        "text": "颜色",
        "bindControlAttr": "pointConfig.color",
        "showType": "dv_color",
        "showExpress": "@[!relationStyle]",
        "spacingNumber": 1,
        "config": {
          "type": "color",
          "noInput": true
        },
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "边框",
        "showExpress": "@[!relationStyle]",
        "bindControlAttr": "pointConfig.borderConfig",
        "showType": "dv_border",
        "config": {
          "isShowTop": false,
          "isShowRight": false,
          "isShowBottom": false,
          "isShowLeft": false
        },
        "itemCss": {
          "position": "relative",
          "left": "26px"
        },
        "inCommonUse": true
      },
      {
        "text": "线",
        "bindControlAttr": "lineConfig.width",
        "showExpress": "@[!relationStyle]",
        "spacingNumber": 1,
        "showType": "dv_number",
        "config": {
          "min": 1
        },
        "itemClass": "float-one-row",
        "itemCss": {
          "position": "relative",
          "left": "40px"
        },
        "inCommonUse": true
      },
      {
        "text": "颜色",
        "bindControlAttr": "lineConfig.color",
        "showType": "dv_color",
        "showExpress": "@[!relationStyle]",
        "spacingNumber": 1,
        "config": {
          "type": "color",
          "noInput": true
        },
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "滚动设置",
    "attributes": [
      {
        "text": "自定义样式",
        "showType": "dv_switch",
        "bindControlAttr": "scrollStyleConfig.isCustomStyle",
        "inCommonUse": true
      },
      {
        "text": "悬浮显示",
        "showType": "dv_switch",
        "showExpress": "@[scrollStyleConfig.isCustomStyle]",
        "bindControlAttr": "scrollStyleConfig.--dv-time-info-scroll-is-h-hover-show",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "宽度/高度",
        "showType": "dv_number",
        "showExpress": "@[scrollStyleConfig.isCustomStyle]",
        "bindControlAttr": "scrollStyleConfig.--dv-time-info-scroll-w-h",
        "spacingNumber": 1,
        "config": {
          "unit": "px"
        },
        "inCommonUse": true
      },
      {
        "text": "小方块圆角",
        "showType": "dv_border_radius",
        "showExpress": "@[scrollStyleConfig.isCustomStyle]",
        "bindControlAttr": "scrollStyleConfig.--dv-time-info-scroll-thumb-border-radius",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "小方块背景",
        "showType": "dv_color",
        "showExpress": "@[scrollStyleConfig.isCustomStyle]",
        "bindControlAttr": "scrollStyleConfig.--dv-time-info-scroll-thumb-bg",
        "spacingNumber": 1,
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "轨道圆角",
        "showType": "dv_border_radius",
        "showExpress": "@[scrollStyleConfig.isCustomStyle]",
        "bindControlAttr": "scrollStyleConfig.--dv-time-info-scroll-track-border-radius",
        "spacingNumber": 1,
        "inCommonUse": true
      },
      {
        "text": "轨道背景",
        "showType": "dv_color",
        "showExpress": "@[scrollStyleConfig.isCustomStyle]",
        "bindControlAttr": "scrollStyleConfig.--dv-time-info-scroll-track-bg",
        "spacingNumber": 1,
        "config": {
          "type": "color"
        },
        "inCommonUse": true
      },
      {
        "text": "自动滚动",
        "showType": "dv_switch",
        "bindControlAttr": "isScroll",
        "inCommonUse": true
      },
      {
        "text": "滚动速度",
        "showExpress": "@[isScroll === true]",
        "spacingNumber": 1,
        "bindControlAttr": "scrollSpeed",
        "inCommonUse": true,
        "showType": "dv_number",
        "desc": "从0.1开始值越大越快",
        "config": {
          "min": 0.1,
          "precision": 1
        }
      }
    ]
  },
  {
    "name": "背景",
    "attributes": [
      {
        "text": "颜色",
        "bindControlAttr": "backgroundConfig",
        "showType": "dv_color",
        "inCommonUse": true
      },
      {
        "text": "图片",
        "bindControlAttr": "backgroundConfig",
        "showType": "dv_more_img",
        "inCommonUse": true,
        "config": {
          "limitImg": 1,
          "backgroundTypes": [
            "page"
          ]
        }
      },
      {
        "text": "外阴影",
        "bindControlAttr": "boxOutsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "outset"
        },
        "inCommonUse": true
      },
      {
        "text": "内阴影",
        "bindControlAttr": "boxInsetShadowConfig",
        "showType": "dv_shadow",
        "config": {
          "subType": "inset"
        },
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件名称",
        "bindControlAttr": "fakeCodeName",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "懒加载",
        "showType": "dv_switch",
        "bindControlAttr": "isHandlerRefresh"
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]