[
  {
    "name": "布局",
    "attributes": [
      {
        "text": "宽",
        "bindControlAttr": "width",
        "showType": "dv_pixel",
        "itemClass": "float-one-row",
        "inCommonUse": true
      },
      {
        "text": "高",
        "bindControlAttr": "height",
        "showType": "dv_pixel",
        "itemClass": "float-one-row ml20",
        "inCommonUse": true
      },
      {
        "text": "边距",
        "showType": "dv_margins_group",
        "inCommonUse": true,
        "config": {
          "options": [
            {
              "text": "内边距",
              "bindControlAttr": "padding"
            },
            {
              "text": "外边距",
              "bindControlAttr": "margin"
            }
          ]
        }
      }
    ]
  },
  {
    "name": "样式",
    "attributes": [
      {
        "text": "直播",
        "bindControlAttr": "isLive",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "直播地址",
        "bindControlAttr": "videoConfig.src",
        "showExpress": "@[isLive]",
        "showType": "dv_text",
        "inCommonUse": true
      },
      {
        "text": "视频封面",
        "bindControlAttr": "backgroundConfig",
        "showType": "dv_more_img",
        "showExpress": "@[!isLive]",
        "inCommonUse": true,
        "config": {
          "limitImg": 1,
          "title": "封面设置",
          "width": "950px",
          "height": "600px",
          "content": "DesDVBgSettingExtend",
          "valueExpress": "!(@[active] == 'pattern' && @[image] == '')"
        }
      },
      {
        "text": "视频上传",
        "bindControlAttr": "videoConfig",
        "showExpress": "@[!isLive]",
        "inCommonUse": true,
        "showType": "dv_video_upload",
        "config": {
          "width": "900px",
          "height": "350px",
          "content": "DesDVVideoUpload",
          "title": "视频上传"
        }
      },
      {
        "text": "视频控件",
        "bindControlAttr": "controlsVideo",
        "showExpress": "@[!isLive]",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "自动播放",
        "bindControlAttr": "autoplay",
        "showExpress": "@[!isLive]",
        "showType": "dv_switch",
        "inCommonUse": true
      },
      {
        "text": "循环播放",
        "bindControlAttr": "loop",
        "showExpress": "@[!isLive]",
        "showType": "dv_switch",
        "inCommonUse": true
      }
    ]
  },
  {
    "name": "高级属性",
    "attributes": [
      {
        "text": "组件标识",
        "bindControlAttr": "caption",
        "showType": "dv_text"
      },
      {
        "text": "组件依赖事件",
        "showType": "dv_dialog",
        "bindControlAttr": "eventRelativeDeal",
        "config": {
          "title": "组件依赖事件",
          "valueExpress": "@[mounted.fn] || @[click.fn] || @[load.fn] || @[dataLoad.fn]",
          "content": "DesDVEventEditor",
          "width": "1200px",
          "height": "60vh",
          "buttonText": "设置"
        }
      },
      {
        "text": "动态属性绑定",
        "bindControlAttr": "development",
        "showType": "dv_dialog",
        "config": {
          "content": "DesDevelopment",
          "width": "1000px",
          "height": "60vh",
          "valueExpress": "!dsf.isEmptyObject(@[attrs]) || @[slots]"
        }
      },
      {
        "text": "数据处理函数",
        "bindControlAttr": "dealDataFunction",
        "showType": "dv_dialog",
        "showExpress": "@[dataType !=='excel'&&dataType !=='static' ]",
        "config": {
          "content": "DesDVDealDataFunction",
          "width": "1000px",
          "height": "400px"
        }
      }
    ]
  },
  {
    "name": "入场动画",
    "attributes": [
      {
        "text": "",
        "hiddenText": true,
        "bindControlAttr": "enterAnimate",
        "showType": "dv_animate_list",
        "showTabType": "animate"
      }
    ]
  }
]